pragma solidity 0.8.15;
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import {MerkleProof} from "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {IERC721} from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import {ERC721A} from "./ERC721A.sol";

contract Spongenuity is ERC721A, Ownable, ReentrancyGuard
{
    string public baseURI = "ipfs://QmYUvXPPVwiJ6XtVw5uqcNdeAoAtR2uDipg5wecZgei89H/";
    address BurnAddress = 0xcff43A597911a9457071d89d2b2AC3D5b1862b86;
    uint Price = 0.5 ether;
    uint A1PrintPrice = 0.15 ether;
    uint InfiniteObjectsPrice = 0.35 ether;
    uint public MAX_SPO = 35;
    bool public PUBLIC_SALE = true;
    bytes32 public Root = 0x779e46cffaa7ec6ac69d6f590a891f4a1a5a300df273ca63d351d08147ec7157;
    mapping(address => bool) public Roles;
    mapping(uint => address) public Redeemed;
    mapping(uint => uint) public TimeSlotRedeemed;
    event Purchased(address Recipient, uint MessageValue, bool A1Print, bool InfiniteObjects);
    event RedeemedMintPass(address Redeemer, uint TokenID, uint TimeSlot);

    /**
     * @dev Constructor 
     */
    constructor() ERC721A("Mint Pass Spongenuity", "MPS") { _mint(0xFAD02e63fFD238F81f66A36498EaCA71fC7317bf, 20); }

    /**
     * @dev Purchases `Spongenuity`
     */
    function Purchase(bool A1Print, bool InfiniteObjects) public payable nonReentrant
    {
        require(PUBLIC_SALE, "Purchaser: Public Sale Inactive");
        require(_totalMinted() + 1 <= MAX_SPO, "Purchaser: Batch Sold Out");
        if(A1Print && InfiniteObjects) { require(msg.value == (Price + A1PrintPrice + InfiniteObjectsPrice), "Purchaser: (A1 & Infinite): Incorrect Message Value"); }
        else if(!A1Print && InfiniteObjects) { require(msg.value == (Price + InfiniteObjectsPrice), "Purchaser: (!A1 & Infinite): Incorrect Message Value"); }
        else if(A1Print && !InfiniteObjects) { require(msg.value == (Price + A1PrintPrice), "Purchaser: (A1 & !Infinite): Incorrect Message Value"); }
        else { require(msg.value == Price, "Purchaser: (!A1 & !Infinite): Incorrect Message Value"); }
        _mint(msg.sender, 1);
        emit Purchased(msg.sender, msg.value, A1Print, InfiniteObjects);
    }

    /**
     * @dev Purchases `Spongenuity`
     */
    function PurchaseBrightList(bool A1Print, bool InfiniteObjects, bytes32[] calldata Proof) public payable nonReentrant
    {
        require(readBrightListMerkle(msg.sender, Proof), "MerkleTree: User Is Not On BrightList");
        require(PUBLIC_SALE, "Purchaser: Public Sale Inactive");
        require(_totalMinted() + 1 <= MAX_SPO, "Purchaser: Batch Sold Out");
        if(A1Print && InfiniteObjects) { require(msg.value == (Price + A1PrintPrice + InfiniteObjectsPrice), "Purchaser: (A1 & Infinite): Incorrect Message Value"); }
        else if(!A1Print && InfiniteObjects) { require(msg.value == (Price + InfiniteObjectsPrice), "Purchaser: (!A1 & Infinite): Incorrect Message Value"); }
        else if(A1Print && !InfiniteObjects) { require(msg.value == (Price + A1PrintPrice), "Purchaser: (A1 & !Infinite): Incorrect Message Value"); }
        else { require(msg.value == Price, "Purchaser: (!A1 & !Infinite): Incorrect Message Value"); }
        _mint(msg.sender, 1);
        emit Purchased(msg.sender, msg.value, A1Print, InfiniteObjects);
    }

    /**
     * @dev Redeems Mint Pass
     */
    function RedeemMintPass(uint TokenID, uint TimeSlot) public nonReentrant
    {
        require(IERC721(address(this)).ownerOf(TokenID) == msg.sender, "Redeemer: User Does Not Own Mint Pass With Input TokenID");
        IERC721(address(this)).transferFrom(msg.sender, BurnAddress, TokenID);
        require(IERC721(address(this)).ownerOf(TokenID) == BurnAddress, "Redeemer: Redeem To Burn Address Failed");
        Redeemed[TokenID] = msg.sender;
        TimeSlotRedeemed[TokenID] = TimeSlot;
        emit RedeemedMintPass(msg.sender, TokenID, TimeSlot);
    }
 
    /****************** 
    *    OnlyOwner    *
    *******************/

    /**
     * @dev Withdraws Ether From Contract To Message Sender
     */
    function __Withdraw() external onlyOwner { payable(msg.sender).transfer(address(this).balance); }

    /**
     * @dev Withdraws Ether From Contract To Address
     */
    function __WithdrawToAddress(address payable recipient) external onlyOwner 
    {
        uint balance = address(this).balance;
        (bool success, ) = recipient.call{value: balance}("");
        require(success, "Unable to Withdraw, Recipient May Have Reverted");
    }

    /**
     * @dev Withdraws ERC20 From Contract To Address
     */
    function __WithdrawERC20ToAddress(address Recipient, address ContractAddress) external onlyOwner
    {
        IERC20 ERC20 = IERC20(ContractAddress);
        ERC20.transferFrom(address(this), Recipient, ERC20.balanceOf(address(this)));
    }

    /**
     * @dev Withdraws ERC721 From Contract To Address
     */
    function __WithdrawERC721sToAddress(address Recipient, uint[] calldata TokenIDs) external onlyBRTOperator
    {
        for(uint x; x < TokenIDs.length; x++)
        {
            IERC721(address(this)).transferFrom(address(this), Recipient, TokenIDs[x]);
        }
    }

    /**
     * @dev Sets Base URI
     */
    function __setBaseURI(string calldata NewBaseURI) external onlyOwner { baseURI = NewBaseURI; }

    /**
     * @dev Changes The Public Sale State
     */
    function __changePublicSaleState() external onlyOwner { PUBLIC_SALE = !PUBLIC_SALE; }

    /**
     * @dev Changes Role
     */
    function __flipRole(address Operator) external onlyOwner { Roles[Operator] = !Roles[Operator]; }

    /**
     * @dev Changes Max SPO
     */
    function __changeMaxSPO(uint NewSPO) external onlyOwner { MAX_SPO = NewSPO; }

    /**
     * @dev Returns If Recipient Address Is BrightListed
     */
    function readBrightListMerkle(address Recipient, bytes32[] memory Proof) public view returns(bool)
    { 
        bytes32 Leaf = keccak256(abi.encodePacked(Recipient));
        return MerkleProof.verify(Proof, Root, Leaf);
    }

    /****************** 
    *  INTERNAL VIEW  *
    ******************/

    /**
     * @dev Returns Base URI
     */
    function _baseURI() internal view virtual override returns (string memory) { return baseURI; }

    /**
     * @dev BRT Operator Modifier
     */
    modifier onlyBRTOperator 
    {
        require(Roles[msg.sender]);
        _;
    }
}
