           }\r\n            if(checkCounter >= 4){\r\n                return true;\r\n            }\r\n        }\r\n        return false;\r\n    }\r\n\r\n    function checkSetAvailability(uint[] calldata ids, uint[] calldata amounts) private view returns(bool){\r\n        for(uint i; i < ids.length; i++) {\r\n            if(totalMintedById(ids[i]) + amounts[i] > AVAILABLE_SUPPLY_FOR_SET){\r\n                return false;\r\n            }\r\n        }\r\n        return true;\r\n    }\r\n\r\n    function checkUltimatePublic() private view returns(bool) {\r\n        for(uint i = TOTAL_SETS; i < TOTAL_IDS; i++) {\r\n            if(balanceOf(msg.sender, i) == 0) {\r\n                return false;\r\n            }\r\n        } \r\n        return true;\r\n    }\r\n\r\n    function checkExclusivePublic(uint id) public view returns(bool) {\r\n        for(uint i; i < 4; i++) {\r\n            uint checkId = _combinations[id][i];\r\n            if(balanceOf(msg.sender, checkId) == 0) {\r\n                return false;\r\n            }\r\n        } \r\n        return true;\r\n    }\r\n\r\n   function checkMicrophonePublic(address claimer) public view returns(bool) {\r\n        uint checkCounter;\r\n        for(uint i = TOTAL_SETS; i < TOTAL_IDS; i++) {\r\n            if(balanceOf(claimer, i) > 0){\r\n                checkCounter++;\r\n            }\r\n            if(checkCounter >= 4){\r\n                return true;\r\n            }\r\n        }\r\n        return false;\r\n    }\r\n\r\n    function checkSetAvailabilityPublic(uint amount, uint id) public view returns(bool){\r\n        if(totalMintedById(id) + amount > AVAILABLE_SUPPLY_FOR_SET){\r\n            return false;\r\n        }else{\r\n            return true;\r\n        }\r\n    }\r\n\r\n    // ---------------------------------------------------------------------------------------------\r\n    // MANAGING FUNCTIONS\r\n    // ---------------------------------------------------------------------------------------------\r\