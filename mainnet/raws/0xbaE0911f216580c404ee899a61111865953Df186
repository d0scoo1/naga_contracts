    }\r\n\r\n    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {\r\n        address owner = _msgSender();\r\n        _approve(owner, spender, _allowances[owner][spender] + addedValue);\r\n        return true;\r\n    }\r\n\r\n    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {\r\n        address owner = _msgSender();\r\n        uint256 currentAllowance = _allowances[owner][spender];\r\n        require(currentAllowance >= subtractedValue, \"ERC20: decreased allowance below zero\");\r\n        unchecked {\r\n            _approve(owner, spender, currentAllowance - subtractedValue);\r\n        }\r\n\r\n        return true;\r\n    }\r\n\r\n    /**\r\n    * @dev Extension of {ERC20} that allows contract owner to set the burn fee .\r\n    * The burn rate is a whole number that will be divided by _burnRateDivider to allow for fractions of a percent. \r\n    * example: burnRate: 15 and burnRateDivider: 100000 will set a burn rate of .00015 or .015%\r\n    */\r\n    function setBurnRate(uint32 _burnRate, uint32 _burnRateDivider) external onlyOwner() {\r\n        require(_burnRate >= 0, \"ERC20: burnRate cannot be negative\");\r\n        require(_burnRateDivider >= 0, \"ERC20: burnRateDivider cannot be negative\");\r\n        if (_burnRate > 0) {\r\n            require(_burnRateDivider > 0, \"ERC20: if burnRate > 0 then burnRateDivider must also be > 0 to prevent div/0 errors\");\r\n            require((_burnRate / _burnRateDivider * 100) < 30 , \"ERC20: burnRate / burnRateDivider must be less than 30%\");\r\n        } else {\r\n            //burn rate == 0, so ensure divider == 0\r\n            require(_burnRateDivider == 0 , \"ERC20: if burnRate == 0 then burnRateDivider must also be 0\");\r\n        }\r\n        burnRate = _burnRate;\r\n        burnRateDivider = _burnRateDivider;        \r\n    }\r\n\r\n    function excludeFromBurn(address account, bool excluded) public onlyO