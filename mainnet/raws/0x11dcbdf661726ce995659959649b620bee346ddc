f ``from``'s tokens\r\n     * will be to transferred to `to`.\r\n     * - when `from` is zero, `amount` tokens will be minted for `to`.\r\n     * - when `to` is zero, `amount` of ``from``'s tokens will be burned.\r\n     * - `from` and `to` are never both zero.\r\n     *\r\n     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].\r\n     */\r\n    function _beforeTokenTransfer(\r\n        address operator,\r\n        address from,\r\n        address to,\r\n        uint256 amount\r\n    ) internal virtual {}\r\n}\r\n\r\n// File: contracts/HoneyToken.sol\r\npragma solidity ^0.8.0;\r\n\r\n/// @title Honey token smart contract\r\n/// @author Aric Kuter\r\ncontract HoneyToken is ERC777 {\r\n    /// @notice equals 20 token per day\r\n    uint256 public immutable DEGENZ_PER_SECOND_PER_DEGEN = 231481 gwei;\r\n    /// @notice To be updated with Genesis deploy time\r\n    uint256 public immutable GENESIS_TIME = 1642177981;\r\n    IERC721Enumerable public immutable GENESIS;\r\n\r\n    mapping(uint256 => uint256) public last;\r\n\r\n    constructor(address _genesisAddr, address[] memory defaultOperators)\r\n        ERC777(\"Honey\", \"HONEY\", defaultOperators)\r\n    {\r\n        GENESIS = IERC721Enumerable(_genesisAddr);\r\n    }\r\n\r\n    function claimable(address user) public view returns (uint256) {\r\n        uint256 total = GENESIS.balanceOf(user);\r\n        uint256 owed = 0;\r\n        for (uint256 i = 0; i < total; i++) {\r\n            uint256 id = GENESIS.tokenOfOwnerByIndex(user, i);\r\n            uint256 claimed = lastClaim(id);\r\n            owed += ((block.timestamp - claimed) * DEGENZ_PER_SECOND_PER_DEGEN);\r\n        }\r\n        return owed;\r\n    }\r\n\r\n    function mintForUser(address user) external {\r\n        uint256 total = GENESIS.balanceOf(user);\r\n        uint256 owed = 0;\r\n        for (uint256 i = 0; i < total; i++) {\r\n            uint256 id = GENESIS.tokenOfOwnerByIndex(user, i);\r\n        