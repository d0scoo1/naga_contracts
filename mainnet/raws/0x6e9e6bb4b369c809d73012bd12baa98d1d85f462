   require(sold <= limit, \"Sale limit reached\");\r\n        if (amount + sold >= limit) amount = limit - sold;\r\n\r\n        require(amount + mintByUser[msg.sender] <= limitPerUser, \"Mint limit passed\");\r\n\r\n        if (freeNFTs >= amount + sold) {\r\n            NFT.safeMint(msg.sender, amount);\r\n            sold += amount;\r\n        } else {\r\n            uint256 paidTokens;\r\n            if (freeNFTs >= sold) {\r\n                paidTokens = sold + amount - freeNFTs;\r\n            } else {\r\n                paidTokens = amount;\r\n            }\r\n\r\n            require(msg.value == paidTokens * price, \"Not enough ether paid\");\r\n            NFT.safeMint(msg.sender, amount);\r\n            sold += amount;\r\n        }\r\n        mintByUser[msg.sender] += amount;\r\n    }\r\n\r\n    //control functions\r\n    function setNFT(address _NFT) external onlyOwner {\r\n        NFT = NFTI(_NFT);\r\n    }\r\n\r\n    function setFreeNFTs(uint256 _freeNFTs) external onlyOwner {\r\n        freeNFTs = _freeNFTs;\r\n    }\r\n\r\n    function setLimit(uint256 _limit) external onlyOwner {\r\n        limit = _limit;\r\n    }\r\n\r\n    function setPrice(uint256 _price) external onlyOwner {\r\n        price = _price;\r\n    }\r\n\r\n    function setLimitPerMint(uint256 _limitPerMint) external onlyOwner {\r\n        limitPerMint = _limitPerMint;\r\n    }\r\n\r\n    function setLimitByUser(uint256 _limit) external onlyOwner {\r\n        limitPerUser = _limit;\r\n    }\r\n\r\n    function setSaleActive(bool _saleActive) external onlyOwner {\r\n        saleActive = _saleActive;\r\n    }\r\n\r\n    // Emergency functions\r\n    function extractValue() external onlyOwner {\r\n        payable(msg.sender).transfer(address(this).balance);\r\n    }\r\n\r\n    function extractToken(uint256 amount, address token) external onlyOwner {\r\n        IERC20(token).transfer(msg.sender, amount);\r\n    }\r\n\r\n    function extractNFT(uint256 NFTID, address token) external onlyOwne