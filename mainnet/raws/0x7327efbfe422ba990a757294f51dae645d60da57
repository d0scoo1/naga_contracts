 to 0.18 ether\");\r\n        require(whitelist[_msgSender()], \"you are not in whitelist\");\r\n        require(!whitelistUserMinted[_msgSender()], \"you have already minted\");\r\n        require(block.timestamp >= whitelistBegin && block.timestamp <= whitelistEnd, \"it's not time for whitelist mint\");\r\n        require(totalSupply() <= 999, \"there's no nft to offer\");\r\n\r\n        whitelistUserMinted[_msgSender()] = true;\r\n        uint256 tokenId = totalSupply() + 1;\r\n\r\n        _safeMint(_msgSender(), tokenId);\r\n        if (birthday != 721) {\r\n            explode(birthday, tokenId);\r\n        } else {\r\n            birth[tokenId] = 721;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * @dev public mint\r\n     */\r\n    function mint(bytes memory signature, int256 birthday) public payable {\r\n        require(msg.value == 0.18 ether, \"value should be equal to 0.18 ether\");\r\n        require(block.timestamp >= mintBegin && block.timestamp <= mintEnd, \"it's not time for public mint\");\r\n        require(totalSupply() <= 999, \"there's no nft to offer\");\r\n        require(QPassprot(QPAddr).addrClaimed(msg.sender) || validate(msg.sender, signature), \"sender has not claimed QPassport nor registered 721Land\");\r\n        uint256 tokenId = totalSupply() + 1;\r\n\r\n        _safeMint(_msgSender(), tokenId);\r\n        if (birthday != 721) {\r\n            explode(birthday, tokenId);\r\n        } else {\r\n            birth[tokenId] = 721;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * @dev calculate the blessings of user\r\n     * @param birthTime birthday timestamp\r\n     * @param tokenId specific tokenId user owned\r\n     */\r\n    function explode(int256 birthTime, uint256 tokenId) public {\r\n        require(ownerOf(tokenId) == _msgSender(), \"only owner can explode the token\");\r\n        require(!exploded[tokenId], \"token can't be exploded again\");\r\n        require(birthTime <= int256(block.timestamp) && birthTime != 721, \"birthday should be earlier than current time\");\r\n        require(tokenExplodedTime[tokenId] + int(24*60*60) <= int(block.timestamp), \"token can't be exploded twice within 24 hours\");\r\n        \r\n        exploded[tokenId] = true;\r\n        birth[tokenId] = birthTime;\r\n        tokenExplodedTime[tokenId] = int(block.timestamp);\r\n\r\n        emit Explode(birthTime, tokenId);\r\n    }\r\n\r\n    /**\r\n     * @dev See {IERC721Metadata-tokenURI}.\r\n     */\r\n    function tokenURI(uint256 tokenId) public override view returns(string memory) {\r\n        require(\r\n            _exists(tokenId),\r\n            \"ERC721Metadata: URI query for nonexistent token\"\r\n        );\r\n\r\n        uint index;\r\n        string memory video_url;\r\n        string memory image_url;\r\n        string memory description;\r\n\r\n        index = getBlessByToken(tokenId);\r\n        video_url = string(abi.encodePacked(arweave_prefix, video[index]));\r\n        image_url = string(abi.encodePacked(arweave_prefix, image[index]));\r\n        if (index == 11) {\r\n            description = string(abi.encodePacked(description_prefix, '.', description_suffix));\r\n        } else {\r\n            description = string(abi.encodePacked(description_prefix, ', ', blessTxt[index], description_suffix));\r\n        }\r\n\r\n        string memory strTokenId = tokenId.toString();\r\n        string memory json = Base64.encode(bytes(string(abi.encodePacked(\r\n            '{\"id\":\"',\r\n            strTokenId,\r\n            '\",\"name\":\"Tiger Year, Tiger Explosions #',\r\n            strTokenId,\r\n            '\",\"image\":\"',\r\n            image_url,\r\n            '\",\"animation_url\":\"',\r\n            video_url,\r\n            '\",\"description\":\"',\r\n            description,\r\n            '\"}'))));\r\n        string memory output = string(abi.encodePacked(\"data:application/json;base64,\",json));\r\n        return output;\r\n    }\r\n\r\n    /**\r\n     * @dev withdraw contract balance\r\n     */\r\n    function withdrawAll() public onlyOwner {\r\n        payable(msg.sender).transfer(address(this).balance);\r\n    }\r\n\r\n    /**\r\n     * @dev get blessing By Token\r\n     */\r\n    function getBlessByToken(uint256 tokenId) public view returns(uint) {\r\n        require(\r\n            _exists(tokenId),\r\n            \"ERC721Metadata: URI query for nonexistent token\"\r\n        );\r\n\r\n        bool isUserTokenExplodeIn2022Flag;\r\n        bool isIn2022Flag;\r\n        bool isUserTigerFlag;\r\n        uint getUserBornYearIndex;\r\n        uint get2022InListIndex = yearBegin.length - 1;\r\n        uint offset;\r\n    \r\n        if (birth[tokenId] != 721) {\r\n            // already explode\r\n            isUserTokenExplodeIn2022Flag = _isUserTokenExplodeIn2022(tokenId);\r\n            isIn2022Flag = _isIn2022();\r\n            (isUserTigerFlag, getUserBornYearIndex) = _isUserTiger(birth[tokenId]);\r\n\r\n            if (isUserTokenExplodeIn2022Flag) {\r\n                // token explode in 2022 year\r\n                if (isIn2022Flag) {\r\n                    // tokenid was exploded in 2022 year and 2022 isn't over now.\r\n                    offset = _getOffset(isUserTigerFlag, get2022InListIndex, getUserBornYearIndex);\r\n                    return offset;\r\n                } else {\r\n                    // 2022 year is over \r\n                    return 11;\r\n                }\r\n            } else {\r\n                // token didn't explode in 2022 year\r\n                offset = _getOffset(isUserTigerFlag, get2022InListIndex, getUserBornYearIndex);\r\n                return offset;\r\n            }\r\n        } else {\r\n            // inintial state\r\n            return 11;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * @dev 721Land validate\r\n     */\r\n    function validate(address user, bytes memory signature) public view returns (bool) {\r\n        require(LandSigner != address(0) && signature.length == 65);\r\n        bytes32 r;\r\n        bytes32 s;\r\n        uint8 v = uint8(signature[64]) + 27;\r\n        assembly {\r\n            r := mload(add(signature, 0x20))\r\n            s := mload(add(signature, 0x40))\r\n        }\r\n        bytes32 hash = keccak256(abi.encode(user));\r\n        return ecrecover(hash, v, r, s) == LandSigner;\r\n    }\r\n\r\n    /**\r\n     * @dev Hook that is called before any transfer of tokens. This includes\r\n     * minting and burning.\r\n     */\r\n    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override {\r\n        super._beforeTokenTransfer(from, to, tokenId);\r\n        exploded[tokenId] = false;\r\n    }\r\n\r\n    /**\r\n     * @dev check if user is tiger or not\r\n     */\r\n    function _isUserTiger(int birthTime) private view returns(bool, uint) {\r\n        for (uint i = 0; i < yearBegin.length; i++) {\r\n            if (birthTime >= yearBegin[i] && birthTime < yearEnd[i]) {\r\n                return (true, i);\r\n            }\r\n        }\r\n        return (false, 999);\r\n    }\r\n\r\n    /**\r\n     * @dev check if now is in 2022  \r\n     */\r\n    function _isIn2022() private view returns(bool) {\r\n        uint get2022InListIndex = yearBegin.length - 1;\r\n        if (int(block.timestamp) >= yearBegin[get2022InListIndex] && int(block.timestamp) < yearEnd[get2022InListIndex]) {\r\n            return true;\r\n        } else {\r\n            return false;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * @dev check if the token explode in 2022\r\n     */\r\n    function _isUserTokenExplodeIn2022(uint256 tokenId) private view returns(bool) {\r\n        uint get2022InListIndex = yearBegin.length - 1;\r\n        if (tokenExplodedTime[tokenId] >= yearBegin[get2022InListIndex] && tokenExplodedTime[tokenId] < yearEnd[get2022InListIndex]) {\r\n            return true;\r\n        } else {\r\n            return false;\r\n        }  \r\n    }\r\n\r\n    /**\r\n     * @dev calculate offset \r\n     */\r\n    function _getOffset(bool isUserTigerFlag, uint beginOfCurrentYearIndex, uint userBornYearIndex) private pure returns(uint) {\r\n        uint offset;\r\n        if (isUserTigerFlag) {\r\n            require(beginOfCurrentYearIndex >= userBornYearIndex, \"birthday should be earlier than current time\");\r\n            offset = beginOfCurrentYearIndex - userBornYearIndex;\r\n            return offset;\r\n        } else {\r\n            // user is not tiger\r\n            return 10;\r\n        }\r\n    }\r\n}", "ABI": "[{\"inputs\":[{\"internalType\":\"address\",\"name\":\"qp\",\"type\":\"address\"},{\"internalType\":\"address\",\"name\":\"signer\",\"type\":\"address\"}],\"stateMutability\":\"nonpayable\",\"type\":\"constructor\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"owner\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"approved\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"uint256\",\"name\":\"tokenId\",\"type\":\"uint256\"}],\"name\":\"Approval\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"owner\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"operator\",\"type\":\"address\"},{\"indexed\":false,\"internalType\":\"bool\",\"name\":\"approved\",\"type\":\"bool\"}],\"name\":\"ApprovalForAll\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":false,\"internalType\":\"int256\",\"name\":\"birthTime\",\"type\":\"int256\"},{\"indexed\":false,\"internalType\":\"uint256\",\"name\":\"tokenId\",\"type\":\"uint256\"}],\"name\":\"Explode\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"previousOwner\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"newOwner\",\"type\":\"address\"}],\"name\":\"OwnershipTransferred\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"in