\n    mapping( address => Claim ) public warmupInfo;\\n\\n    /**\\n        @notice stake Sigma to enter warmup\\n        @param _amount uint\\n        @return bool\\n     */\\n    function stake( uint _amount, address _recipient ) external returns ( bool ) {\\n        rebase();\\n        \\n        Sigma.safeTransferFrom( msg.sender, address(this), _amount );\\n\\n        Claim memory info = warmupInfo[ _recipient ];\\n        require( !info.lock, \\\"Deposits for account are locked\\\" );\\n\\n        warmupInfo[ _recipient ] = Claim ({\\n            deposit: info.deposit.add( _amount ),\\n            gons: info.gons.add( Alpha.gonsForBalance( _amount ) ),\\n            expiry: epoch.number.add( warmupPeriod ),\\n            lock: false\\n        });\\n        \\n        Alpha.safeTransfer( address(warmupContract), _amount );\\n        emit LogStake(_recipient, _amount);\\n        return true;\\n    }\\n\\n    /**\\n        @notice retrieve ALPHA from warmup\\n        @param _recipient address\\n     */\\n    function claim ( address _recipient ) external {\\n        Claim memory info = warmupInfo[ _recipient ];\\n        if ( epoch.number >= info.expiry && info.expiry != 0 ) {\\n            delete warmupInfo[ _recipient ];\\n            uint256 amount = Alpha.balanceForGons( info.gons );\\n            warmupContract.retrieve( _recipient,  amount);\\n            emit LogClaim(_recipient, amount);\\n        }\\n    }\\n\\n    /**\\n        @notice forfeit ALPHA in warmup and retrieve Sigma\\n     */\\n    function forfeit() external {\\n        Claim memory info = warmupInfo[ msg.sender ];\\n        delete warmupInfo[ msg.sender ];\\n        uint memoBalance = Alpha.balanceForGons( info.gons );\\n        warmupContract.retrieve( address(this),  memoBalance);\\n        Sigma.safeTransfer( msg.sender, info.deposit);\\n        emit LogForfeit(msg.sender, memoBalance, info.deposit);\\n    }\\n\\n    /**\\n        @notice prevent new deposits to address (protection from