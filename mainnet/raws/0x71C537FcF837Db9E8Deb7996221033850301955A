n        emit FundsReleased(_treasury, amount);\r\n        payable(_treasury).sendValue(amount);\r\n    }\r\n\r\n\r\n    /**\r\n     * @dev Returns how much royalty is owed and to whom, based on a sale price that may be denominated in any unit of\r\n     * exchange. The royalty amount is denominated and should be payed in that same unit of exchange.\r\n     */\r\n    function royaltyInfo(uint256 /*tokenId*/, uint256 salePrice) \r\n        external view \r\n        returns (address receiver, uint256 royaltyAmount)\r\n    {\r\n        if (_royaltyReceiver == address(0) || _royaltyBps == 0) {\r\n            return (address(0), 0);\r\n        }\r\n        uint256 royaltyNumerator = salePrice * _royaltyBps;\r\n        royaltyAmount = royaltyNumerator / MAX_BPS;\r\n        receiver = _royaltyReceiver;\r\n    }\r\n\r\n    function _setRoyalty(address newRoyaltyReceiver, uint16 newRoyaltyBps)\r\n        internal\r\n    {\r\n        _royaltyReceiver = newRoyaltyReceiver;\r\n        _royaltyBps = newRoyaltyBps;\r\n    }\r\n\r\n    function setRoyalty(address newRoyaltyReceiver, uint16 newRoyaltyBps)\r\n        external onlyRole(DEFAULT_ADMIN_ROLE)\r\n    {\r\n        _setRoyalty(newRoyaltyReceiver, newRoyaltyBps);\r\n    }\r\n\r\n\r\n    /*==============================================================\r\n        Metadata\r\n    --------------------------------------------------------------*/\r\n    /**\r\n     * @dev URI for contract level metadata used by OpenSea.  \r\n     * See https://docs.opensea.io/docs/contract-level-metadata\r\n     */\r\n    function contractURI() \r\n        external view \r\n        returns (string memory)\r\n    {\r\n        return _contractURI;\r\n    }\r\n\r\n    /// @dev Lets a contract admin set the URI for contract-level metadata.\r\n    function setContractURI(string calldata uri) \r\n        external onlyRole(DEFAULT_ADMIN_ROLE)\r\n    {\r\n        _contractURI = uri;\r\n    }\r\n\r\n    /// @dev Returns the URI for a given tokenId.\r\n    function tokenURI(uint256 tokenId) \r\n        public view \r\n        override \r\n        returns (string memory)\r\n    {\r\n        if (tokenId < _startTokenId() && tokenId >= _currentIndex) revert URIQueryForNonexistentToken();\r\n        string memory baseURI = _baseURI();\r\n        if (bytes(baseURI).length > 0) {\r\n            if (_revealed) {\r\n                return string(abi.encodePacked(baseURI, tokenId.toString()));\r\n            } else {\r\n                return string(abi.encodePacked(baseURI, \"0\"));\r\n            }\r\n        }\r\n        return \"\";\r\n    }\r\n\r\n    /**\r\n     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each\r\n     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty\r\n     * by default, can be overriden in child contracts.\r\n     */\r\n    function _baseURI() \r\n        internal view virtual override \r\n        returns (string memory)\r\n    {\r\n        return _metadataBaseURI;\r\n    }\r\n\r\n    /// @dev Lets a contract admin set the base URI for token metadata.\r\n    function setBaseURI(string calldata uri) \r\n        external onlyRole(DEFAULT_ADMIN_ROLE)\r\n    {\r\n        _metadataBaseURI = uri;\r\n    }\r\n\r\n    function _encryptedBaseURI() \r\n        internal view virtual \r\n        returns (bytes memory)\r\n    {\r\n        return _encryptedMetadataBaseURI;\r\n    }\r\n\r\n    /// @dev Lets a contract admin update an encrypted base URI used for a reveal.  Reverts if already revealed.\r\n    function setEncryptedBaseURI(bytes calldata uri) \r\n        external onlyRole(DEFAULT_ADMIN_ROLE)\r\n    {\r\n        if (_revealed) revert AlreadyRevealed();\r\n        _encryptedMetadataBaseURI = uri;\r\n    }\r\n\r\n    function maxSupply() \r\n        external view returns (uint256)\r\n    {\r\n        return _maxSupply;\r\n    }\r\n\r\n    /// @dev Lets an account with `DEFAULT_ADMIN_ROLE` reveal the URI for a batch of 'delayed-reveal' NFTs.\r\n    function reveal(bytes calldata key)\r\n        external\r\n        onlyRole(DEFAULT_ADMIN_ROLE)\r\n        returns (string memory revealedURI)\r\n    {\r\n        if (_revealed) revert AlreadyRevealed();\r\n        bytes memory encryptedURI = _encryptedMetadataBaseURI;\r\n        if (encryptedURI.length == 0) revert EncryptedUriNotSet();\r\n\r\n        revealedURI = string(LibEncryptDecrypt.encryptDecrypt(encryptedURI, key));\r\n        _metadataBaseURI = revealedURI;\r\n        delete _encryptedMetadataBaseURI;\r\n        _revealed = true;\r\n        emit NFTRevealed(revealedURI);\r\n\r\n        return revealedURI;\r\n    }\r\n\r\n    function isRevealed()\r\n        external view returns (bool)\r\n    {\r\n        return _revealed;\r\n    }\r\n\r\n\r\n    /*==============================================================\r\n        Minting and Claims\r\n    --------------------------------------------------------------*/\r\n    function configureAllowListSale(AllowListSaleConfig calldata newConfig) \r\n        external onlyRole(DEFAULT_ADMIN_ROLE)\r\n    {\r\n        if (newConfig.startTime == 0 \r\n                || newConfig.endTime == 0 \r\n                || newConfig.startTime > newConfig.endTime\r\n                || newConfig.merkleRoot == BYTES32_ZERO) \r\n            revert InvalidSaleConfiguration();\r\n\r\n        emit AllowListSaleConfigUpdated(_allowListSaleConfig, newConfig);\r\n        _allowListSaleConfig = newConfig;\r\n    }\r\n\r\n    function allowListSaleConfig() \r\n        external view \r\n        returns (AllowListSaleConfig memory)\r\n    {\r\n        return _allowListSaleConfig;\r\n    }\r\n\r\n    function configurePublicSale(PublicSaleConfig calldata newConfig) \r\n        external onlyRole(DEFAULT_ADMIN_ROLE)\r\n    {\r\n        if (newConfig.startTime == 0 \r\n                || (newConfig.endTime > 0 && newConfig.startTime > newConfig.endTime)) \r\n            revert InvalidSaleConfiguration();\r\n\r\n        emit PublicSaleConfigUpdated(_publicSaleConfig, newConfig);\r\n        _publicSaleConfig = newConfig;\r\n    }\r\n\r\n    function publicSaleConfig() \r\n        external view \r\n        returns (PublicSaleConfig memory)\r\n    {\r\n        return _publicSaleConfig;\r\n    }\r\n\r\n    /**\r\n     * @dev Mints `quantity` tokens and transfers them to `to`.\r\n     *\r\n     * Requirements:\r\n     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called for each safe transfer.\r\n     *   `quantity` must be greater than 0.\r\n     *\r\n     * Emits a {Transfer} event.\r\n     */\r\n    function purchase(uint64 quantity) \r\n        external virtual \r\n        nonReentrant\r\n        payable\r\n    {\r\n        // Prevent bots\r\n        require(tx.origin == msg.sender);\r\n\r\n        // Verify public mint phase\r\n        if (block.timestamp < _publicSaleConfig.startTime \r\n                || (_publicSaleConfig.endTime > 0 && block.timestamp > _publicSaleConfig.endTime)) \r\n            revert MintPhaseNotActive();\r\n\r\n        address to = _msgSender();\r\n\r\n        // Verify sufficient suppply\r\n        if (_totalMinted() + quantity > _maxSupply)\r\n            revert InsufficientSupply();\r\n        uint64 currPurchaseQty = _getAux(to);\r\n        if (currPurchaseQty + quantity > _publicSaleConfig.purchaseLimit)\r\n            revert ExceedsPurchaseLimit();\r\n\r\n        // Verify payment amount\r\n        uint256 totalPrice = 0;\r\n        if (_publicSaleConfig.price > 0) {\r\n            totalPrice = quantity * _publicSaleConfig.price;\r\n            if (msg.value < totalPrice) \r\n                revert InsufficientValue();\r\n        }\r\n\r\n        // Mint tokens to msg sender\r\n        _setAux(to, currPurchaseQty + quantity);\r\n        emit Purchase(to, quantity, _currentIndex);\r\n        _mint(to, quantity, '', false);\r\n    }\r\n\r\n    function numberPurchased(address addr) external view returns (uint64) {\r\n        return _getAux(addr);\r\n    }\r\n\r\n    /**\r\n     * Returns the number of tokens minted by `addr`.\r\n     */\r\n    function numberMinted(address addr) external view returns (uint256) {\r\n        return _numberMinted(addr);\r\n    }\r\n\r\n    /**\r\n     * @dev Claims a quantity of tokens for an allowed address.\r\n     *\r\n     * Requirements:\r\n     * - msg.sender is in the allowed address list\r\n     * - proofMaxQuantity is total number of mints allowed by the claimer\r\n     * - quantity + totalMinter(claimer) is less than or equal to proofMaxQuantity\r\n     *\r\n     * Emits {Transfer} and {TokensClaimed} events.\r\n     */\r\n    function claim(\r\n        uint64 quantity,\r\n        uint256 proofMaxQuantity,\r\n        bytes32[] calldata proofs\r\n    )\r\n        external payable virtual\r\n        nonReentrant\r\n    {\r\n        // Prevent bots\r\n        require(tx.origin == msg.sender);\r\n\r\n        // Verify allow list mint phase\r\n        if (block.timestamp < _allowListSaleConfig.startTime || block.timestamp > _allowListSaleConfig.endTime) \r\n            revert MintPhaseNotActive();\r\n\r\n        // Validate parameters\r\n        address claimer = _msgSender();\r\n        if (quantity == 0) revert InvalidClaimQuantity();\r\n        if (proofs.length == 0) revert InvalidClaimProof();\r\n\r\n        // Verify merkle proof for inclusion in allowed address list\r\n        verifyClaimMerkleProof(\r\n            claimer,\r\n            proofMaxQuantity,\r\n            proofs\r\n        );\r\n\r\n        // Verify claim quantity\r\n        uint256 numberClaimed = _numberMinted(claimer) - _getAux(claimer);\r\n        if (numberClaimed + quantity  > proofMaxQuantity) \r\n            revert InvalidClaimQuantity();\r\n        if (_totalMinted() + quantity > _maxSupply)\r\n            revert InsufficientSupply();\r\n\r\n        // Verify payment amount\r\n        uint256 totalPrice = 0;\r\n        if (_allowListSaleConfig.price > 0) {\r\n            totalPrice = quantity * _allowListSaleConfig.price;\r\n            if (msg.value < totalPrice) revert InsufficientValue();\r\n        }\r\n\r\n        // Mint tokens to claimer and emit event\r\n        emit Purchase(claimer, quantity, _currentIndex);\r\n        _mint(claimer, quantity, '', false);\r\n    }\r\n\r\n    /// @dev Checks whether a claimer meets the claim condition's allowlist criteria.\r\n    function verifyClaimMerkleProof(\r\n        address claimer,\r\n        uint256 proofMaxQuantity,\r\n        bytes32[] calldata proofs\r\n    )\r\n        public view \r\n        returns (bool validMerkleProof)\r\n    {\r\n        if (_allowListSaleConfig.merkleRoot != bytes32(0)) {\r\n            validMerkleProof = MerkleProofUpgradeable.verify(\r\n                proofs,\r\n                _allowListSaleConfig.merkleRoot,\r\n                keccak256(abi.encode(claimer, proofMaxQuantity))\r\n            );\r\n            if (!validMerkleProof) revert InvalidClaimProof();\r\n        }\r\n    }\r\n}", "ABI": "[{\"inputs\":[],\"name\":\"AlreadyRevealed\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"ApprovalCallerNotOwnerNorApproved\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"ApprovalQueryForNonexistentToken\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"ApprovalToCurrentOwner\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"ApproveToCaller\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"BalanceQueryForZeroAddress\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"EncryptedUriNotSet\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"ExceedsPurchaseLimit\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InsufficientSupply\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InsufficientValue\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InvalidAddress\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InvalidClaimProof\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InvalidClaimQuantity\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InvalidSaleConfiguration\",\"type\":\"er