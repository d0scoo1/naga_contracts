\r\n        _minted += amount;\r\n    }\r\n\r\n\r\n\r\n    modifier validateProof(bytes32[] calldata _merkleProof) {\r\n        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));\r\n        if (!MerkleProof.verify(_merkleProof, _merkleRoot, leaf)) {\r\n            revert NotInWhitelist();\r\n        }\r\n        _;\r\n    }\r\n\r\n\r\n    function getPrivateSalePrice() public view returns (uint256){\r\n        return _privateSalePrice;\r\n    }\r\n}", "ABI": "[{\"inputs\":[{\"internalType\":\"address payable\",\"name\":\"_walletAddress\",\"type\":\"address\"},{\"internalType\":\"uint256\",\"name\":\"privateSalePrice_\",\"type\":\"uint256\"},{\"internalType\":\"bytes32\",\"name\":\"_initialMerkleRoot\",\"type\":\"bytes32\"},{\"internalType\":\"uint256\",\"name\":\"totalSupply_\",\"type\":\"uint256\"},{\"internalType\":\"contract NFT\",\"name\":\"_nftAddress\",\"type\":\"address\"},{\"internalType\":\"uint256\",\"name\":\"privateSaleStartedAt_\",\"type\":\"uint256\"},{\"internalType\":\"uint256\",\"name\":\"privateSaleStoppedAt_\",\"type\":\"uint256\"}],\"stateMutability\":\"nonpayable\",\"type\":\"constructor\"},{\"inputs\":[],\"name\":\"CantSendEthersToWallet\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"InvalidAmountOfEthers\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"LimitExceeded\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"NotInWhitelist\",\"type\":\"error\"},{\"inputs\":[],\"name\":\"PrivateSaleNotStarted\",\"type\":\"error\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"previousOwner\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"newOwner\",\"type\":\"address\"}],\"name\":\"OwnershipTransferred\",\"type\":\"event\"},{\"inputs\":[{\"internalType\":\"uint256\",\"name\":\"_startedAt\",\"type\":\"uint256\"},{\"internalType\":\"uint256\",\"name\":\"_stoppedAt\",\"type\":\"uint256\"}],\"name\":\"changeSaleTime\",\"outputs\":[],\"stateMutability\":\"nonpayable\",\"type\":\"