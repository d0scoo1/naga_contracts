.0 staking service\r\ncontract KelePoolStaking is Initializable, UUPSUpgradeable {\r\n    // constants\r\n    uint8 public constant MAX_VALIDATORS = 100; // validators\r\n    uint32 public constant PUBKEY_LENGTH = 48; // public key length\r\n    uint32 public constant SIGNATURE_LENGTH = 96; // signature length\r\n    uint32 public constant CREDENTIALS_LENGTH = 32; // credentials length\r\n    uint256 public constant DEPOSIT_AMOUNT = 32 ether; // deposit amount\r\n    address public constant ETH2_DEPOSIT_ADDRESS =\r\n        0x00000000219ab540356cBB839Cbe05303d7705Fa; // eth2.0 contract address 0x00000000219ab540356cBB839Cbe05303d7705Fa\r\n\r\n    // system info\r\n    struct System {\r\n        bool status; // status\r\n        address owner; // manager\r\n        address operator; // operator\r\n        address foundation; // foundation\r\n        uint256 fee; // fee 0.05 ETH\r\n        uint256 minimum; // deposit minimum 0.01\r\n        uint256 balance; // deposit amount\r\n        uint256 devfee; // dev fee\r\n    }\r\n    System private _system;\r\n    mapping(address => uint256) public micros;\r\n    mapping(address => uint256) public whales;\r\n\r\n    // events\r\n    event OnDeposit(address indexed payer, uint256 amount, uint256 time);\r\n    event OnWithdraw(\r\n        address indexed owner,\r\n        address indexed receiver,\r\n        uint256 amount\r\n    );\r\n    event OnValidatorCreated(\r\n        address indexed owner,\r\n        uint8 role,\r\n        uint256 amount,\r\n        uint256 time,\r\n        bytes withdrawal_credentials,\r\n        bytes pubkey\r\n    );\r\n    event OnFeeTakeOut(\r\n        address indexed owner,\r\n        address indexed receiver,\r\n        uint256 amount\r\n    );\r\n    event OnFeeChanged(address indexed owner, uint256 amount);\r\n    event OnStatusChanged(address indexed owner, bool status);\r\n    event OnOwnerChanged(address indexed owner, address ownership);\r\n    event OnOperatorChanged(address indexed own