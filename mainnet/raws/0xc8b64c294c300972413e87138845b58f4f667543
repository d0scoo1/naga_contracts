\r\n        else if(_potionID == 9){\r\n            require(msg.value == PRICE4, MSG_NOT_ENOUGHT_BALANCE);\r\n            require(monster1.reflex <= 250, MSG_YOU_ALREADY_STRONG);\r\n            monster1.reflex += 300;\r\n        }\r\n        else if(_potionID == 10){\r\n            require(msg.value == PRICE4, MSG_NOT_ENOUGHT_BALANCE);\r\n\r\n            uint randomchance = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 3;\r\n            nonce++;\r\n            if(randomchance == 1){\r\n                uint randomspeed = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 100;\r\n                nonce++;\r\n                monster1.reflex += randomspeed;\r\n            } else if(randomchance == 2){\r\n                uint randomattack = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 100;\r\n                nonce++;\r\n                monster1.strength += randomattack;\r\n            } else{\r\n                uint randomdefense = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 100;\r\n                nonce++;\r\n                monster1.stamina += randomdefense;\r\n            }\r\n        }\r\n    }\r\n   \r\n    //change NFT name\r\n    function changeNFTName (uint _monsterID, string memory _newName) public nonReentrant {\r\n        require(ownerOf(_monsterID) == msg.sender);\r\n        EachMonster storage monster1 =  monsters[_monsterID];\r\n        monster1.name = _newName;\r\n    }\r\n\r\n    function price(uint256 _count) public view returns (uint256) {\r\n        return PRICE.mul(_count);\r\n    }\r\n    \r\n    // Make it possible to change the price: just in case\r\n    function setPrice(uint256 _newPrice) external onlyOwner {\r\n        PRICE = _newPrice;\r\n    }\r\n\r\n    function _baseURI() internal view virtual override returns (string memory) {\r\n        return baseTokenURI;\r\n    }\r\n\r\n    function setBaseURI(string memory baseURI) public onlyOwner {\r