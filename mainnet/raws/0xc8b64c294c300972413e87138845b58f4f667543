r\n            b.winnerId = b.p1CarId;\r\n            b.ended = 1;\r\n        } else if (b.scoreP2 > b.scoreP1) {\r\n            b.winnerId = b.p2CarId;\r\n            b.ended = 1;\r\n        } else {\r\n            b.ended = 1;\r\n        }\r\n    }\r\n\r\n    //end battle\r\n    function endBattle (uint _battleId) public nonReentrant{\r\n        Battle storage b = savas[_battleId];\r\n        require(b.ended == 1,MSG_ERROR);\r\n        if(b.scoreP2 <= b.scoreP1) {\r\n            require(ownerOf(b.p1CarId) == msg.sender);\r\n            upgradeWinner(b.p1CarId,1);\r\n            upgradeLoser(b.p2CarId);\r\n            b.ended = 2;\r\n        } else if (b.scoreP2 > b.scoreP1) {\r\n            require(ownerOf(b.p2CarId) == msg.sender);\r\n            upgradeWinner(b.p2CarId,2);\r\n            upgradeLoser(b.p1CarId);\r\n            b.ended = 2;\r\n        }\r\n        clearBattleElement(_battleId);\r\n    }\r\n\r\n    // Start Resting to earn Health based on user desirable period\r\n    function startRest(uint256 _tokenId, uint256 day) public {\r\n        EachMonster storage m = monsters[_tokenId];\r\n        require(ownerOf(_tokenId) == msg.sender, MSG_ONLY_OWNER_NFT_REST);\r\n        require(m.training == false, MSG_NFT_ALREADY_RESTING);\r\n        uint256 until = day*86400;\r\n\r\n        m.training = true;\r\n        m.unlockDate = until + block.timestamp;\r\n        m.lockedDay = day;\r\n    }\r\n\r\n    // Stop the Resting and release NFT\r\n    function leaveRest(uint256 _tokenId) public {\r\n        EachMonster storage m = monsters[_tokenId];\r\n        require(m.training == true, MSG_NFT_NOT_RESTING);\r\n        require(ownerOf(_tokenId) == msg.sender, MSG_ONLY_OWNER_NFT_NOT_REST);\r\n        \r\n        require(block.timestamp >= m.unlockDate, MSG_TOKENS_STILL_LOCKED);\r\n        uint addedHealth = m.lockedDay*2;\r\n        m.health += addedHealth;\r\n        m.training = false;\r\n        m.unlockDate = 0;\r\n        m.lockedDay = 0;\r\n    }\r\n\r\n    // Move the last element to the deleted spot.\r\n    // Remove the last element.\r\n    function clearBattleElement(uint index) internal {\r\n        require(index < savas.length);\r\n        savas[index] = savas[savas.length-1];\r\n        savas.pop();\r\n    }\r\n\r\n    //upgrade chimeras\r\n    function upgradeWinner(uint _carId, uint condition) internal {\r\n        EachMonster storage m = monsters[_carId];\r\n        m.winnings += 1;\r\n        if(condition == 1){\r\n         m.stamina += 5;\r\n        }else{\r\n         m.strength += 5; \r\n        }\r\n        m.reflex += 1;  \r\n    }\r\n\r\n    function upgradeLoser(uint _carId) internal {\r\n        EachMonster storage m = monsters[_carId];\r\n        m.losings += 1;\r\n        uint randomchance = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 3;\r\n        nonce++;\r\n        if(randomchance == 1){\r\n            if (m.strength > 0) {\r\n                m.strength -= 1;\r\n            } else if (m.strength == 0) {\r\n                m.strength = 0;\r\n            }\r\n        } else if(randomchance == 2){\r\n            if (m.stamina > 0) {\r\n                m.stamina -= 1;\r\n            } else if (m.stamina == 0) {\r\n                m.stamina = 0;\r\n            }\r\n        } else{\r\n            if (m.reflex > 0) {\r\n                m.reflex -= 1;\r\n            } else if (m.reflex == 0) {\r\n                m.reflex = 0;\r\n            }\r\n        }\r\n\r\n        if (m.health > 0) {\r\n            m.health -= 1;\r\n        } else if (m.health == 0) {\r\n            m.health = 0;\r\n        }\r\n    }\r\n\r\n    //get battle counts\r\n    function getBattlesCount() public view returns(uint) {\r\n        return savas.length;\r\n    }\r\n\r\n    function _totalSupply() internal view returns (uint256) {\r\n        return _tokenIdTracker.current();\r\n    }\r\n\r\n    function totalMint() public view returns (uint256) {\r\n        return _totalSupply();\r\n    }\r\n\r\n    uint nonce = 0;\r\n    function random() internal returns (uint) {\r\n        uint randomnumber = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 750;\r\n        randomnumber = randomnumber + 100;\r\n        nonce++;\r\n        return randomnumber;\r\n    }\r\n\r\n    function setMainAddress(address _royaltyAddress, uint256 _royaltyValue, address _creatorAddress) public onlyOwner {\r\n        _setRoyalties(_royaltyAddress, _royaltyValue);\r\n        creatorAddress = _creatorAddress;\r\n    }\r\n\r\n    function mint(string memory _name, address _to, uint256 _count) public payable saleIsOpen nonReentrant{\r\n        uint256 total = _totalSupply();\r\n        uint256 balance = balanceOf(_to);\r\n        bool isOwner = _msgSender() == owner();\r\n        bool isWhite = isPresale ? whitelist[_to] : true;\r\n        require(total + _count <= MAX_ELEMENTS, MSG_MAX_LIMIT);\r\n        require(total <= MAX_ELEMENTS, MSG_SALE_END);\r\n        require(isOwner || _count <= MAX_BY_MINT, MSG_EXCEEDS_NUMBER);\r\n        require(isOwner || balance < MAX_BY_ADDRESS, MSG_EXCEEDS_BY_ADDRESS);\r\n        require(isOwner || msg.value >= price(_count), MSG_VALUE_BELOW_PRICE);\r\n        require(isOwner || isWhite, MSG_ARRAY_NOT_WHITELIST);\r\n\r\n        for (uint256 i = 0; i < _count; i++) {\r\n            _mintAnElement(_name, _to);\r\n        }\r\n    }\r\n\r\n    function _mintAnElement(string memory _name, address _to) private {\r\n        uint256 id = _totalSupply();\r\n        _tokenIdTracker.increment();\r\n        _safeMint(_to, id);\r\n        emit CreateChimera(id);\r\n        idToOwner[id] = msg.sender; \r\n        monsters.push(EachMonster(_name, 0, 0, 10, random(), random(), random(), random(), false, 0, 0, false )); \r\n    }\r\n\r\n    function getPotionPrices() public view returns (uint256, uint256, uint256, uint256){\r\n        return (PRICE1,PRICE2,PRICE3,PRICE4);\r\n    }\r\n    \r\n    function setPotionPrices(uint256 _newPotion1,uint256 _newPoti