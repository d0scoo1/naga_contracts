      for( uint i; i < (owned_NFT_count[msg.sender] - owned_protected_count[msg.sender]); i+step) {\r\n           if(!owned_NFT_Ids[msg.sender][i]) {\r\n               step=0;\r\n           } else {\r\n               /// @dev Avoid transferring ////important tokens\r\n               if(!isProtectedNFToken(i)) {\r\n                  NFTtransferFrom(msg.sender,_to,i);\r\n                  transferred+=1;\r\n                  step=1;\r\n                  if(transferred==_value) {\r\n                      return _success;\r\n                  }\r\n               }\r\n           }\r\n       }\r\n       return _success;\r\n    }\r\n    \r\n   /*\r\n   Protection implementation\r\n\r\n   Is intended to be either set by the user or by an hypotetic platform based\r\n   for example on rarity or special events. Protection blocks NFToken transfer.\r\n   */\r\n\r\n    /// @dev Awareness on protection\r\n    function isProtectedNFToken(uint tokenId) public view override returns(bool is_protected) {\r\n      return tokenProtection[tokenId];\r\n    }\r\n\r\n   /// @dev Protection manipulation\r\n   function setNFTokenProtection(uint tokenId, bool protection_value) public onlyAuth {\r\n     // Avoid gas waste and update protection count\r\n     if(protection_value) {\r\n       require(!tokenProtection[tokenId]);\r\n       owned_protected_count[ownerOf(tokenId)] += 1;\r\n     } else {\r\n       require(tokenProtection[tokenId]);\r\n       owned_protected_count[ownerOf(tokenId)] -= 1;\r\n     }\r\n     tokenProtection[tokenId] = protection_value;\r\n   }\r\n\r\n   function setOwnedNFTokenProtection(uint tokenId, bool protection_value) public {\r\n     require(ownerOf(tokenId)==msg.sender, \"You don't own the token\");\r\n     // Avoid gas waste and update protection count\r\n     if(protection_value) {\r\n       require(!tokenProtection[tokenId]);\r\n       owned_protected_count[ownerOf(tokenId)] += 1;\r\n     } else {\r\n       require(tokenProtection[tokenId]);\r\n       owned_protected_count[ownerOf(tokenId)] -= 1;\r\n     }\r\n     tokenProtection[tokenId] = protection_value;\r\n   }\r\n\r\n    /* Admin Functions */\r\n\r\n\r\n    /// @dev Native token paired price for target\r\n    function getTokenPrice(uint amount) public view returns(uint)\r\n    {\r\n            \r\n                (uint Res0, uint Res1,uint timestamp) = pair.getReserves();\r\n                if(Res0==0 && Res1==0) {\r\n                  return 0;\r\n                }\r\n                uint res0 = Res0;\r\n                delete timestamp;\r\n                return((amount*res0)/Res1); // return amount of token0 needed to buy token1\r\n    }\r\n\r\n    uint coinPrice;\r\n\r\n\r\n  /* ==================================================================================================== */\r\n  /* ==================================================================================================== */\r\n  /* ============================================ ERC20 Part ============================================ */\r\n  /* ==================================================================================================== */\r\n  /* ==================================================================================================== */\r\n\r\n    /*\r\n    This part defines all the ERC20 standard properties\r\n    */\r\n\r\n  address Dead = 0x000000000000000000000000000000000000dEaD;\r\n\r\n  uint256 constant private MAX_UINT256 = 2**256 - 1;\r\n    mapping (address => uint256) public balances;\r\n    mapping (address => mapping (address => uint256)) public allowed;\r\n    uint256 public ERCtotalSupply;\r\n    /*\r\n    NOTE:\r\n    The following variables are OPTIONAL vanities. One does not have to include them.\r\n    They allow one to customise the token contract & in no way influences the core functionality.\r\n    Some wallets/interfaces might not even bother to look at this information.\r\n    */\r\n    string public name = \"The Altairian Dollar\";                   //fanc