a7a38bf8c91e23\r\n    ///  - bzz://mydomain.eth/land.png\r\n    ///  - https://cdn.mydomain.com/land.png\r\n    /// Images should be valid PNG.\r\n    function publish(uint _idx, string _link, string _image, string _title, bool _NSFW) {\r\n        Land storage land = lands[_idx];\r\n        require(msg.sender == land.owner);\r\n        land.link = _link;\r\n        land.image = _image;\r\n        land.title = _title;\r\n        land.NSFW = _NSFW;\r\n\r\n        Publish(_idx, land.link, land.image, land.title, land.NSFW || land.forceNSFW);\r\n    }\r\n\r\n    /// setLandOwner changes the owner of an land unit\r\n    function setLandOwner(uint _idx, address _newOwner) {\r\n        Land storage land = lands[_idx];\r\n        require(msg.sender == land.owner);\r\n        land.owner = _newOwner;\r\n\r\n        SetLandOwner(_idx, msg.sender, _newOwner);\r\n    }\r\n\r\n    /// forceNSFW allows the owner to override the NSFW status for a specific Land unit.\r\n    function forceNSFW(uint _idx, bool _NSFW) {\r\n        require(msg.sender == contractOwner);\r\n        Land storage land = lands[_idx];\r\n        land.forceNSFW = _NSFW;\r\n\r\n        Publish(_idx, land.link, land.image, land.title, land.NSFW || land.forceNSFW);\r\n    }\r\n\r\n    /// withdraw allows the owner to transfer out the balance of the contract.\r\n    function withdraw() {\r\n        require(msg.sender == contractOwner);\r\n        withdrawWallet.transfer(this.balance);\r\n    }\r\n}", "ABI": "[{\"constant\":false,\"inputs\":[{\"name\":\"_x\",\"type\":\"uint256\"},{\"name\":\"_y\",\"type\":\"uint256\"},{\"name\":\"_width\",\"type\":\"uint256\"},{\"name\":\"_height\",\"type\":\"uint256\"}],\"name\":\"buy\",\"outputs\":[{\"name\":\"idx\",\"type\":\"uint256\"}],\"payable\":true,\"stateMutability\":\"payable\",\"type\":\"function\"},{\"constant\":true,\"inputs\":[],\"name\":\"pixelsPerCell\",\"outputs\":[{\"name\":\"\",\"type\":\"uint256\"}],\"payable\":false,\"stateMutability\":\"view\",\"type\":\"func