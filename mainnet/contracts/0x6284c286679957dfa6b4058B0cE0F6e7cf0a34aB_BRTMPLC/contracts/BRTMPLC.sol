contract BRTMPLC is ERC20, Ownable
{
    constructor() ERC20("BRT | Emily Xie", "BRT | Emily Xie") 
    {
        _mint(0xBAc93dB39A4d4ec2c679582c6519B8cba018Dc92, 100 ether); // Mints 100 BRTMPLC to LiveMint Smart Contract
        _transferOwnership(0xB96E81f80b3AEEf65CB6d0E280b15FD5DBE71937); // Transfers Ownership To BRT Multisig
    }

    function ownerMint(address Recipient, uint Amount) external onlyOwner
    {
        _mint(Recipient, Amount);
    }
}