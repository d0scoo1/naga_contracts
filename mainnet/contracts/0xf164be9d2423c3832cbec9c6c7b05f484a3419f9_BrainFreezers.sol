contract BrainFreezers is ERC721A, Ownable, Pausable, ReentrancyGuard {
    using Strings for uint256;

    bool public saleIsActive;
    string private BASE_URI; //METADATA
    string public baseExtension = ".json";
    bool public isFreezeListActive = false;
    uint256 public MAX_SUPPLY;
    uint256 public MAX_PUBLIC_MINT;
    uint256 public PRICE_PER_TOKEN;


    mapping(address => uint8) private _freezeList;


    constructor(uint256 price, uint256 maxSupply, uint256 maxMint, string memory baseUri, bool isSaleActive) ERC721A("Brain Freezers", "BF") {
        PRICE_PER_TOKEN = price;
        MAX_SUPPLY = maxSupply;
        MAX_PUBLIC_MINT = maxMint;
        BASE_URI = baseUri;
        saleIsActive = isSaleActive;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function setIsFreezeListActive(bool _isFreezeListActive) external onlyOwner {
        isFreezeListActive = _isFreezeListActive;
    }

    function setFreezeList(address[] calldata addresses, uint8 numAllowedToMint) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; i++) {
            _freezeList[addresses[i]] = numAllowedToMint;
        }
    }

    function numAvailableToMint(address addr) external view returns (uint8) {
        return _freezeList[addr];
    }

    function mintFreezeList(uint8 numberOfTokens) external payable {
        uint256 ts = totalSupply();
        require(isFreezeListActive, "Freezelist sale is not active");
        require(numberOfTokens <= _freezeList[msg.sender], "Exceeded max available to purchase");
        require(ts + numberOfTokens <= MAX_SUPPLY, "Purchase would exceed max tokens");
        require(PRICE_PER_TOKEN * numberOfTokens <= msg.value, "Insufficient payment");

        _freezeList[msg.sender] -= numberOfTokens;
        _safeMint(msg.sender, numberOfTokens);
    }

    function setBaseURI(string memory customBaseURI_) external onlyOwner {
        BASE_URI = customBaseURI_;
    }

    function _baseURI() internal view virtual override(ERC721A) returns (string memory) {
        return BASE_URI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory)
    {
    require(_exists(tokenId),"ERC721AMetadata: URI query for nonexistent token");
    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
        : "";
    }

    function devReserve(uint256 n) public onlyOwner {
        uint256 ts = totalSupply();
        require(ts + n <= MAX_SUPPLY, "Mint would exceed max tokens");
        _safeMint(msg.sender, n);
    }

    function setPrice(uint256 customPrice) external onlyOwner {
        PRICE_PER_TOKEN = customPrice;
    }

    function mintGiveaway(address _to, uint numberOfTokens) public onlyOwner {
        uint256 ts = totalSupply();
        require(ts + numberOfTokens <= MAX_SUPPLY, "Mint would exceed max tokens");
        _safeMint(_to, numberOfTokens);
    }

    function setSaleState(bool newState) public onlyOwner {
        saleIsActive = newState;
    }

    function mint(uint numberOfTokens) public payable {
        uint256 ts = totalSupply();
        require(saleIsActive, "Sale must be active to mint tokens");
        require(numberOfTokens <= MAX_PUBLIC_MINT, "Exceeded max token purchase");
        require(ts + numberOfTokens <= MAX_SUPPLY, "Purchase would exceed max tokens");
        require(PRICE_PER_TOKEN * numberOfTokens <= msg.value, "Insufficient payment");

        _safeMint(msg.sender, numberOfTokens);
    }

    //OWNER WALLET
    address private constant payoutAddress1 = 0x5ABc60aA3c3978749DF8C6821e05fc28aeD71134;
    //COMMUNITY WALLET
    address private constant payoutAddress2 = 0x32E26C086c6A0E56306E9410eAa28FDb7Bd4f0C0;

    function withdraw() public onlyOwner nonReentrant {
        uint256 balance = address(this).balance;

        Address.sendValue(payable(payoutAddress1), balance / 2);
        Address.sendValue(payable(payoutAddress2), balance / 2);
    }
}