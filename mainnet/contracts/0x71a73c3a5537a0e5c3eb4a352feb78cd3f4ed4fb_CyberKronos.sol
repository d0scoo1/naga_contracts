    // internal
    function _baseURI() internal view virtual override returns (string memory) {
      return baseURI;
    }


    // public
    function mint(uint8 _mintAmount) public payable {
        require(!paused(), "Pausable: paused");                  //requires paused to be false
        require(_mintAmount > 0, "Cannot Mint 0 NFTs");         
        require(totalSupply() + _mintAmount <= maxSupply, "Max supply has been reached");
    
        //project owner mints for free
        if (msg.sender != projectOwner && msg.sender != developer) {
            if (tier1WL[msg.sender] == true  && tier1WLCanMint == true) {
                require(_mintAmount <= maxTier1WLMintAmount);
                require(msg.value >= tier1WLCost * _mintAmount);
            }
            else {
                require(publicCanMint, "Public Mint has not started"); 
                require(_mintAmount <= maxPublicMintAmount);
                require(msg.value >= publicCost * _mintAmount);
            }
        }

        //mints the number of nfts specified by user
        _safeMint(msg.sender, _mintAmount);
        
    }



    //returns all tokens that account owns
    function walletOfOwner(address _owner) public view returns (uint256[] memory) {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory ownedTokenIds = new uint256[](ownerTokenCount);
        uint256 currentTokenId = 1;
        uint256 ownedTokenIndex = 0;

        while (ownedTokenIndex < ownerTokenCount && currentTokenId <= maxSupply) {
            address currentTokenOwner = ownerOf(currentTokenId);

            if (currentTokenOwner == _owner) {
                ownedTokenIds[ownedTokenIndex] = currentTokenId;
                ownedTokenIndex++;
            }
            currentTokenId++;
        }

        return ownedTokenIds;
    }


    function batchTier1WL(address[66] memory _addresses) internal {
        require(whiteListAdmin[msg.sender] == true, "You are not a White List Manager or above");
        for (uint256 i = 0; i < _addresses.length; i++) {
            tier1WL[_addresses[i]] = true;
        }
    }

    function addTier1WL(address _newWhiteListed) public {
        require(whiteListAdmin[msg.sender] == true, "You are not a White List Manager or above");
        tier1WL[_newWhiteListed] = true; //adds specifed role to address
    }
  
    function removeTier1WL(address _whiteListed) public {
        require(whiteListAdmin[msg.sender] == true, "You are not a White List Manager or above");
        tier1WL[_whiteListed] = false; //adds specifed role to address
    }


    //sets the ipfs links just in case someone got it wrong the first time -_-
    function setBaseURI(string memory _newBaseURI) public {
        require(contractAdmin[msg.sender] == true, "You are not a Contract Manager or above");
        baseURI = _newBaseURI;  
    }


    function startTier1Mint() public {
        require(contractAdmin[msg.sender] == true, "You are not a Contract Manager or above");
        tier1WLCanMint = true;
    }

    function stopTier1Mint() public {
        require(contractAdmin[msg.sender] == true, "You are not a Contract Manager or above");
        tier1WLCanMint = false;
    }

    function startPublicMint() public {
        require(contractAdmin[msg.sender] == true, "You are not a Contract Manager or above");
        publicCanMint = true;
    }


    //just-in-case functions
    function pause() public {
        require(contractAdmin[msg.sender] == true, "You are not a Contract Manager or above");
        _pause();
    }

    function unpause() public {
        require(contractAdmin[msg.sender] == true, "You are not a Contract Manager or above");
        _unpause();
    }

    
    // only developer
    function addContractAdmin(address _newContractAdmin) public {
        require(msg.sender == developer, "You are not the developer");
        contractAdmin[_newContractAdmin] = true;
    }
  
    function removeContractAdmin(address _contractAdmin) public {
        require(msg.sender == developer, "You are not the developer");
        contractAdmin[_contractAdmin] = false;
    }

    function addWhiteListAdmin(address _newWhiteListAdmin) public {
        require(msg.sender == developer, "You are not the developer");
        whiteListAdmin[_newWhiteListAdmin] = true;
    }
  
    function removeWhiteListAdmin(address _whiteListAdmin) public {
        require(msg.sender == developer, "You are not the developer");
        whiteListAdmin[_whiteListAdmin] = false;
    }



    // only project owner
    function setTier1WLCost(uint256 _newTier1WLCost) public {
        require(msg.sender == projectOwner, "You are not the project owner");
        tier1WLCost = _newTier1WLCost;
    }

    function setPublicCost(uint256 _newPublicCost) public {
        require(msg.sender == projectOwner, "You are not the project owner");
        publicCost = _newPublicCost;
    }


    function setMaxTier1WLMintAmount(uint8 _newMaxTier1WLMintAmount) public {
        require(msg.sender == projectOwner, "You are not the project owner");
        maxTier1WLMintAmount = _newMaxTier1WLMintAmount;
