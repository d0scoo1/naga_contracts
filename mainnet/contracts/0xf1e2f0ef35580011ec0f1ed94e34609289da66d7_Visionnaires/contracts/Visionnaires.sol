pragma solidity ^0.8.4;

import '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import '@openzeppelin/contracts/access/Ownable.sol';

contract Visionnaires is ERC721, Ownable {
    uint256 public mintPrice;
    uint256 public totalSupply;
    uint256 public maxSupply;
    string internal baseTokenUri;

    //Artist: 
    address public lapinWallet;
    address public tagachiWallet;
    address public daoWallet;

    address public rocketLadyWallet;

    mapping(address => uint256) public WalletMints;

    constructor() payable ERC721('The Visionnaires', 'VSN') {
        mintPrice = 0.7 ether; 
        totalSupply = 0;
        maxSupply = 17;

        //Arist: Lapin Mignon - https://twitter.com/Lelapinmignon
        lapinWallet = 0x114F78f4342ae3dCBfAF61B6BF19807678754224;

        //Coder: Tagachi - https://twitter.com/TagachiStudio
        tagachiWallet = 0xff88Cd0b8c1300ef503b9df32789050B02f26f4B;

        //DAO: Mignonverse - https://twitter.com/mignonverse
        daoWallet = 0xEF247Fb4A0bb76398472af7729d9819bB446e4BB;

        //Rocket Lady: Renate Frotscher - https://twitter.com/Renatefrotscher
        rocketLadyWallet = 0xdE090027BF758dE3Ba32078F14E91F1A263818ab;

        //Minting Tokens for founders
        _safeMint(lapinWallet, 1);
        WalletMints[lapinWallet]++;
        totalSupply++;

        _safeMint(tagachiWallet, 2);
        WalletMints[tagachiWallet]++;
        totalSupply++;

    }


    //Change Wallets - Failsafe
    function changeLapinWallet(address lapinWallet_) external onlyOwner {
        lapinWallet = lapinWallet_;
    }

    function changeTagachiWallet(address tagachiWallet_) external onlyOwner {
        tagachiWallet = tagachiWallet_;
    }

    function changeDaoWallet(address daoWallet_) external onlyOwner {
        daoWallet = daoWallet_;
    }

    //Token URI change
    function setBaseTokenUri(string calldata baseTokenUri_) external onlyOwner {
        baseTokenUri = baseTokenUri_;
    }

    function tokenURI(uint256 tokenId_) public view override returns (string memory) {
        require(_exists(tokenId_), 'Token does not exist!');
        return string(abi.encodePacked(baseTokenUri, Strings.toString(tokenId_), ".json"));
    }

    //Withdrawal pattern
    function withdraw() external onlyOwner {

        uint256 _totalWithdrawal = address(this).balance;
        uint256 _totalWithdrawalShare = _totalWithdrawal / 3;
        
        uint256 _lapinWallet = _totalWithdrawalShare;
        uint256 _tagachiWallet = _totalWithdrawalShare;
        uint256 _daoWallet = _totalWithdrawal - _lapinWallet - _tagachiWallet;

        (bool successLapin, ) = lapinWallet.call{ value: _lapinWallet }('');
        require(successLapin, 'withdraw to Lapin failed');
        (bool successTagachi, ) = tagachiWallet.call{ value: _tagachiWallet }('');
        require(successTagachi, 'withdraw to Tagachi failed');
        (bool successDao, ) = daoWallet.call{ value: _daoWallet }('');
        require(successDao, 'withdraw to Dao failed');

    }

    //The Visionnaire funding
    function mint(uint256 quantity_) public payable {
        require(msg.value >= quantity_ * mintPrice, 'wrong mint value');
        require(totalSupply + quantity_ <= maxSupply, 'sold out');

        for(uint256 i = 0; i < quantity_; i++) {
            uint256 newTokenId = totalSupply + 1;
            WalletMints[msg.sender]++;
            totalSupply++;
            _safeMint(msg.sender,newTokenId);
        }
    }

    //Commissioning Rocket
    function commissionRocket() external onlyOwner {
        uint256 newTokenId = totalSupply + 1;
        WalletMints[rocketLadyWallet]++;
        totalSupply++;
        _safeMint(rocketLadyWallet, newTokenId);
    }
}