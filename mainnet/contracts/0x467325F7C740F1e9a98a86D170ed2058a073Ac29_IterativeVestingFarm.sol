// File contracts/IterativeVestingFarm.sol

pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;





contract IterativeVestingFarm is OwnableUpgradeable, ReentrancyGuardUpgradeable{

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // Info about each user
    struct Reward{
        // Address of user
        address user;
        // Amount of user
        uint256 amount;
    }

    // Time when farm is starting to vest
    uint256 public startTime;
    // Time when farm is ending with vesting
    uint256 public endTime;
    // 1/vestingPortion of totalUserRewards
    uint256 public nVestingPortions;
    // On how many days is portion distributed
    uint256 public distributionIntervalDays;
    // Array of all reward times during factory
    uint256[] public vestingTimes;
    // Address of token that is vested
    IERC20 public vestedToken;
    // Total rewards that needs to be paid out
    uint256 public totalRewards;
    // Total rewards that we owe to the users
    uint256 public totalRewardsPendingBalance;
    // Mapping with reward amount,
    // that should be paid out for all users
    mapping(address => uint256) public totalUserRewards;
    // Activation of farm
    bool public isActive;
    // Mapping with users last withdrawals
    mapping(address => uint256) public userLastWithdrawnPortionNumber;
    // Did user collected first reward or not
    mapping(address => bool) public withdrawFirstPortion;
    // How many tokens we gave to user
    mapping(address => uint256) public totalUserPayouts;
    // Array of users
    address[] public participants;
    // Mapping of users id
    mapping(address => uint256) public usersId;
    // Number of users
    uint256 public noOfUsers;
    // Is farm user
    mapping(address => bool) public isFarmUser;
    // Iterative farm implementation
    address public farmImplementation;
    // Total rewards withdrawn
    uint256 totalWithdrawn;
    // Is user removed
    mapping(address => bool) public isRemoved;

    // Events
    event RewardPaid(
        address indexed user,
        uint256 indexed reward
    );
    event EmergencyWithdraw(
        address indexed _asset,
        uint256 indexed _amount,
        address indexed _owner
    );
    event UserRemoved(
        address indexed user,
        uint256 indexed totalRewards,
        uint256 indexed totalPayouts,
        uint256 balanceWhenRemoved
    );
    event LeftOverTokensRemoved(
        uint256 indexed amountWithdrawn,
        address indexed collector,
        uint256 indexed balance,
        uint256 pendingAmount
    );
    event StartFarm(bool indexed _isActive);
    event PauseFarm(bool indexed _isActive);

    /**
     * @notice function is initializing contract
     *
     * @param _vestedToken - address of vested token
     * @param _startTime - time when farm starts
     * @param _nVestingPortions - amount that is distributed of whole amount
     * @param _distributionIntervalDays - on how many days,
     * reward is distributed to users
     */
    function initialize(
        address _vestedToken,
        uint256 _startTime,
        uint256 _nVestingPortions,
        uint256 _distributionIntervalDays,
        address _farmImplementation
    )
        external
        initializer
    {
        __Ownable_init();
        __ReentrancyGuard_init();

        require(
            _vestedToken != address(0x0),
            "Tokens address can not be 0x0"
        );
        require(
            _startTime >= block.timestamp,
            "Start time can not be in the past"
        );
        require(
            _nVestingPortions > 0,
            "Portion of distribution has to be over 0"
        );

        uint256 distributionPeriod;
        if(_nVestingPortions == 1){
            distributionPeriod = 1;
        }
        else{
            require(
                _distributionIntervalDays > 0,
                "Delay between two vesting days needs to be over 0"
            );

            distributionPeriod = _distributionIntervalDays;
        }

        vestedToken = IERC20(_vestedToken);
        startTime = _startTime;
        nVestingPortions = _nVestingPortions;
        distributionIntervalDays = distributionPeriod;
        endTime = startTime +
            ((nVestingPortions - 1) * distributionIntervalDays * 24 * 60 * 60);
        isActive = false;

        uint256 rewardTime;
        for(uint256 i = 0; i < nVestingPortions; i++){
            if(i == 0){
                rewardTime = startTime;
                vestingTimes.push(rewardTime);
            }
            else{
                rewardTime = rewardTime +
                    (distributionIntervalDays * 24 * 60 * 60);
                vestingTimes.push(rewardTime);
            }
        }

        farmImplementation = _farmImplementation;
    }

    // All state changing functions

    /**
     * @notice function is adding users into the array
     *
     * @param _rewards - array of [userAddress, userAmount]
     *
     * @dev this is function that creates data,
     * to work with
     */
    function addUsersRewards(
        Reward[] calldata _rewards
    )
        external
        onlyOwner
    {
        require(
            !isActive,
            "Farm is activated you can not add more users"
        );

        for(uint256 i = 0 ; i < _rewards.length; i++){
            Reward calldata r = _rewards[i];
            if(r.amount > 0 && !isFarmUser[r.user]){
                totalRewards = totalRewards
                    .add(r.amount)
                    .sub(totalUserRewards[r.user]);
                totalRewardsPendingBalance = totalRewardsPendingBalance
                    .add(r.amount)
                    .sub(totalUserRewards[r.user]);

                participants.push(r.user);
                usersId[r.user] = noOfUsers;
                noOfUsers++;
                totalUserPayouts[r.user] = 0;
                userLastWithdrawnPortionNumber[r.user] = 0;
                withdrawFirstPortion[r.user] = false;
                totalUserRewards[r.user] = r.amount;
                isFarmUser[r.user] = true;
            }
        }
    }

    /**
     * @notice function is removing user from farm
     *
     * @param user - address of user,
     * that needs to be removed
     */
    function removeUser(
        address user
    )
        external
        onlyOwner
    {
        require(
            !isActive,
            "Iterative farm is activated, you can't remove user"
        );
        require(
            totalUserRewards[user] > totalUserPayouts[user],
            "User can not be kicked, he is already paid"
        );

        uint256 removedUserPendingBalance =
            totalUserRewards[user].sub(totalUserPayouts[user]);
        // update totalRewards
        totalRewards = totalRewards.sub(removedUserPendingBalance);
        // update totalRewardsPendingBalance
        totalRewardsPendingBalance = totalRewardsPendingBalance.sub(removedUserPendingBalance);
        // remove him
        totalUserRewards[user] = totalUserPayouts[user];
        isRemoved[user] = true;

        emit UserRemoved(
            user,
            totalUserRewards[user],
            totalUserPayouts[user],
            removedUserPendingBalance
        );
    }

    /**
     * @notice function is funding the farm
     */
    function fundAndOrActivate()
        external
        onlyOwner
    {
        if(totalRewardsPendingBalance > vestedToken.balanceOf(address(this))) {
            uint256 amount =
                totalRewardsPendingBalance - vestedToken.balanceOf(address(this));

            vestedToken.safeTransferFrom(
                address(msg.sender),
                address(this),
                amount
            );
        }

        require(
            vestedToken.balanceOf(address(this)) >= totalRewardsPendingBalance,
            "There is not enough money to payout all users"
        );

        isActive = true;
        emit StartFarm(isActive);
    }

    /**
     * @notice function is stopping farm
     */
    function pauseFarm()
        external
        onlyOwner
    {
        isActive = false;
        emit PauseFarm(isActive);
    }

    // All view functions

    /**
     * @notice returns total rewards,
     * that are locked,unlocked and withdrawn
     *
     * @return totalRewardsLocked,totalRewardsUnlocked
     * and totalWithdrawn
     */
    function getTotalRewardsLockedUnlockedAndWithdrawn()
        external
        view
        returns(uint256, uint256, uint256)
    {
        if(nVestingPortions != 1){
            uint256 rewardPerPortion = (totalRewards / nVestingPortions);
            uint256 nPortionsVested = (
                (
                    (
                        block.timestamp - startTime
                    ) / 86400
                ) / distributionIntervalDays
            );

            if(nPortionsVested > nVestingPortions - 1){
                nPortionsVested = nVestingPortions - 1;
            }

            nPortionsVested += 1;
            uint256 totalRewardsUnlocked = rewardPerPortion * nPortionsVested;
            uint256 totalRewardsLocked = totalRewards - totalRewardsUnlocked;
            return (
                totalRewardsLocked,
                totalRewardsUnlocked,
                totalWithdrawn
            );
        }
        else{
            return (0, totalRewards, totalWithdrawn);
        }
    }

    // All withdraw functions

    /**
     * @notice function is giving back rewards
     */
    function withdraw()
        external
        nonReentrant
    {
        require(
            block.timestamp > startTime,
            "Farm has not started yet"
        );
        require(
            isActive,
            "Farm is not active"
        );
        require(
            totalUserPayouts[address(msg.sender)] < totalUserRewards[address(msg.sender)],
            "User has been paid out"
        );

        uint256 amountEarned;
        uint256 rewardPerPortion = (totalUserRewards[address(msg.sender)] / nVestingPortions);
        uint256 nPortionsVested = (
            (
                (
                    block.timestamp - startTime
                ) / 86400
            ) / distributionIntervalDays
        );

        if(nPortionsVested > nVestingPortions - 1){
            nPortionsVested = nVestingPortions - 1;
        }

        if(withdrawFirstPortion[address(msg.sender)]){
            require(
                userLastWithdrawnPortionNumber[address(msg.sender)] < nPortionsVested,
                "User withdrew everything available at this moment"
            );
        }

        amountEarned = (
            (
                (nPortionsVested - userLastWithdrawnPortionNumber[address(msg.sender)])
            ) * (totalUserRewards[address(msg.sender)] / nVestingPortions)
        );

        if (amountEarned > 0)
            userLastWithdrawnPortionNumber[address(msg.sender)] = nPortionsVested;

        if(!withdrawFirstPortion[address(msg.sender)]){
            amountEarned = amountEarned + rewardPerPortion;
            withdrawFirstPortion[address(msg.sender)] = true;
        }

        if (amountEarned > 0){
            totalUserPayouts[address(msg.sender)] += amountEarned;
            totalRewardsPendingBalance -= amountEarned;
            vestedToken.safeTransfer(address(msg.sender), amountEarned);
            emit RewardPaid(address(msg.sender), amountEarned);

            // Update total withdrawn
            totalWithdrawn += amountEarned;
        }
    }

    /**
     * @notice function is collecting,
     * superfluous rewards
     *
     * @param collector - address of client
     */
    function removeLeftOverRewards(
        address collector
    )
        external
        onlyOwner
    {
        require(
            block.timestamp > endTime,
            "Farm is not finished yet"
        );
        require(
            vestedToken.balanceOf(address(this)) > totalRewardsPendingBalance,
            "There is no superfluous tokens on factory"
        );

        uint256 withdrawnAmount =
            vestedToken.balanceOf(address(this)).sub(totalRewardsPendingBalance);

        vestedToken.safeTransfer(collector, withdrawnAmount);

        emit LeftOverTokensRemoved(
            withdrawnAmount,
            collector,
            vestedToken.balanceOf(address(this)),
            totalRewardsPendingBalance
        );
    }

    /**
     * @notice function in case of emergency
     * is withdrawing on account of factory
     *
     * @param asset - address of token
     * @param collector - address of congress
     */
    function emergencyAssetsWithdrawal(
        address asset,
        address collector
    )
        external
        onlyOwner
    {
        require(
            !isActive,
            "Farm is active you can't emergency withdraw"
        );
        require(
            asset != address(0x0),
            "Address of token can not be 0x0 address"
        );
        require(
            collector != address(0x0),
            "Collector can not be 0x0 address"
        );

        IERC20 token = IERC20(asset);
        uint256 amount = token.balanceOf((address(this)));
        token.safeTransfer(collector, token.balanceOf((address(this))));
        isActive = false;
        emit EmergencyWithdraw(asset, amount, address(msg.sender));
    }
}