    mapping(uint256 => address) public burnToken;

    event shelfOn(address indexed from, uint256 indexed tokenId, uint256 price);
    event shelfOff(address indexed from, uint256 indexed tokenId);
    event exchange (uint256 indexed tokenId, uint256 indexed qu, address indexed spawner, uint256 price);
    event combined (uint256 indexed tokenId, bool indexed isLegend, address addr, bytes tokenIds, uint256 price);
    event buySCF (address indexed from, address indexed to, uint256 tokenId, uint256 price, uint256 tax);
    event giftSCF (address indexed from, address indexed to, uint256 tokenId);

    constructor() ERC721Full("SpaceCapsuleFragment", "SCF") public {}
    // 碎片盲盒开启
    function mint (uint256 tokenId, uint256 qu, bytes memory signature, uint nonce) public payable {
        require(verify2(msg.sender, tokenId, qu, nonce, signature), "Invalid signature"); // 验证签名
        require(msg.value == mint_price, "incorrect value");
        require(burnToken[tokenId] == address(0), "token has been burned");

        token_quality[tokenId] = qu == 1 ? true : false;
        emit exchange(tokenId, qu, msg.sender, msg.value);
        _safeMint(msg.sender, tokenId);
    }
    /*
     * @dev 合并太空碎片
     * @param tokenId 
     */
    function combine(uint256[10] memory tokenIds) public payable returns(bool) {
        require(SCAddr != address(0), "Combine does not start yet");
        require(combine_price == msg.value, 'value dose not match');
        bool[10] memory flag = [false, false, false, false, false, false, false, false, false, false];

        bytes memory combinedTokenIds = abi.encodePacked(tokenIds);
        address combinedAddr = msg.sender;
        uint256 legend_prob = 0;
        for (uint8 i = 0; i < tokenIds.length; i++) {
            // 判断尾号，数组flag的索引对应了尾号0-9，值对应了出现的次数
            // 如果索引（尾号）对应的值不为0，就代表已经出现过，返回错误！
            uint256 appendage = uint256(tokenIds[i] % 10);
            require(flag[appendage] == false, "The fragment combination rule is incorrect");
            flag[appendage] = true;
            
            legend_prob += token_quality[tokenIds[i]] ? 10 : 0; // 根据该碎片师傅是高品质，确认开出传奇盲盒的概率
            // 烧掉对应的token id
            _burn(msg.sender, tokenIds[i]);
            burnToken[tokenIds[i]] = msg.sender;
            actionToken[tokenIds[i]] = 0;
        }

        // 调用太空舱合约的铸造方法
        ISC sc = ISC(SCAddr);
        (uint256 tokenId, bool isLengedBox) = sc.mintWithWhitlist(msg.sender, legend_prob);
        
        emit combined(tokenId, isLengedBox, combinedAddr, combinedTokenIds, msg.value);
        return isLengedBox; // 合成是否拿到了传奇盲盒
    }


    // =====================================下面的方法都不是主逻辑，隔开，方便阅读========================================

    // 设置太空舱合约地址
    function setSCAddr(address sc_addr) onlyOwner public {
        require(sc_addr.isContract(), "The given address is not a contract");
        SCAddr = sc_addr;
    }

    function acutionUp(uint256 tokenId, uint256 price) public {
        require(ownerOf(tokenId) == msg.sender, 'not the owner of nft');
        require(price > 0, 'incorrect price');
        actionToken[tokenId] = price;
        emit shelfOn(msg.sender, tokenId, price);
    }

    function acutionDown(uint256 tokenId) public {
        require(ownerOf(tokenId) == msg.sender, 'not the owner of nft');
        require(actionToken[tokenId] > 0, 'nft not on sale');
        actionToken[tokenId] = 0;
        emit shelfOff(msg.sender, tokenId);
    }

    function buy(uint256 tokenId) public payable {
        require(actionToken[tokenId] > 0, 'nft not on sale');
        require(actionToken[tokenId] == msg.value, 'incorrect value');
        address owner = ownerOf(tokenId);
        address to = msg.sender;
        uint256 tax = actionToken[tokenId];
        tax = tax.mul(425);
        tax = tax.div(10000);
        uint256 transfer_fee = msg.value - tax;
        if (transfer_fee > 0) {
            address(uint160(owner)).transfer(transfer_fee);
        }
        _transferFrom(owner, to, tokenId);

        emit buySCF(msg.sender, to, tokenId, msg.value, tax);
    }

    function gift(address to, uint256 tokenId) public {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _transferFrom(msg.sender, to, tokenId);
        emit giftSCF(msg.sender, to, tokenId);
    }

    function getCombinePrice() public view returns (uint256) {
        return combine_price;
    }

    function setCombinePrice(uint256 p) public onlyCFO {
        combine_price = p;
    }

    function getMintPrice() public view returns (uint256) {
        return mint_price;
    }

    function setMintPrice(uint256 p) public onlyCFO {
        mint_price = p;
    }

    function withdraw(uint256 balance, address addr) onlyCFO public {
        require(balance <= getTotalBalance(), 'not enought balance');
        require(addr != address(0), 'address did not set yet');
        address(uint160(addr)).transfer(balance);
    }

    function getTotalBalance() onlyCFO public view returns(uint256) {
        return address(this).balance;
    }

    function _transferFrom(address from, address to, uint256 tokenId) internal {
        actionToken[tokenId] = 0;
        super._transferFrom(from, to, tokenId);
    }
}