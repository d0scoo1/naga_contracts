        excludeFromMaxTransaction(address(uniswapV2Pair), true);
        _setAutomatedMarketMakerPair(address(uniswapV2Pair), true);

        // 18 zeroes
        uint256 totalSupply = 5_000_000_000 * 1e18;

        maxTransactionAmount = totalSupply.div(100); // 1% from total supply maxTransactionAmountTxn
        maxWallet = totalSupply.div(50); // 2% from total supply maxWallet
        swapTokensAtAmount = (totalSupply * 5) / 10000; // 0.05% swap wallet
        // 11%
        buyTotalFees = 1100;
        // double sell tax for 12h
        sellTotalFees = 2200;

        marketingWallet = address(0x5F8dE52e9780FE96CE33b755efBDD6a0b4AAE439); // set as marketing wallet
        devWallet = address(0x5EFc0Af78F4fCe85C1fEBd9D82EfdfEE73680F8a); // set as dev wallet
        taxGasThreshold = 200000;

