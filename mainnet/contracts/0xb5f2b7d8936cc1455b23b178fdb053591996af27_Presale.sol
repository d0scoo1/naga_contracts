contract TokenHolder is Ownable {
  function withdraw(address token) onlyOwner external {
    IERC20 erc20 = IERC20(token);
    erc20.transfer(_msgSender(), erc20.balanceOf(address(this)));
  }
}

contract Presale is Ownable {
  using ECDSA for bytes32;

  struct Price {
    uint56 multiplier;
    uint8 divider;
  }

  address constant SIGNER = 0x303353275c411449A92F5509766F5b8a815282C2;

  Price private _whitelistPrice;
  Price private _publicPrice;
  address private _token;
  address private _quoteToken;
  address private _router;
  address[] private _tokenHolders;

  mapping (address => uint256) public nonce;
  uint256 public participants;
  address[] public whitelistParticipants;
  address[] public publicParticipants;
  mapping (address => uint256) public whitelistDeposits;
  mapping (address => uint256) public publicDeposits;
  bool public finalized;
  uint256 public timelock;

  event Deposited(address indexed account, uint256 amount, bool whitelist);
  event Finalized();
  event Claimed(address indexed account);
  event Unlocking(uint256 timelock);
  event Migrated(address indexed recipient);

  constructor() {
    if (block.chainid == 1) { // Ethereum
      _whitelistPrice = Price(1e15, 9);
      _publicPrice = Price(1e14, 1);
      _token = 0x1Fa34D9C32049f2Cd775693904CFEBB9CfA3927E;
      _quoteToken = 0xdAC17F958D2ee523a2206206994597C13D831ec7;
      _router = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    } else if (block.chainid == 56) { // Binance Smart Chain
      _whitelistPrice = Price(1e3, 9);
      _publicPrice = Price(1e2, 1);
      _token = 0x1Fa34D9C32049f2Cd775693904CFEBB9CfA3927E;
      _quoteToken = 0x55d398326f99059fF775485246999027B3197955;
      _router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    } else {
      revert("Presale: invalid chain");
    }

    for (uint8 i = 0; i < 2; i++) {
      _tokenHolders.push(address(new TokenHolder()));
    }
  }

  function _verifySignature(bytes memory abiEncoded, bytes memory signature) private pure {
    require(keccak256(abiEncoded).toEthSignedMessageHash().recover(signature) == SIGNER, "Presale: invalid signature");
  }

  function whitelistParticipantsLength() external view returns (uint256) {
    return whitelistParticipants.length;
  }

  function publicParticipantsLength() external view returns (uint256) {
    return publicParticipants.length;
  }

  function deposit(uint256 amount, bool whitelist, uint256 _nonce, bytes memory signature) external payable {
    address sender = _msgSender();
    _verifySignature(abi.encode(block.chainid, address(this), sender, "deposit", whitelist, _nonce), signature);
    require(_nonce == nonce[sender], "Presale: invalid nonce");
    nonce[sender]++;
    require(!finalized, "Presale: finalized");
    IERC20 quoteToken = IERC20(_quoteToken);
    uint256 balance = quoteToken.balanceOf(address(this));

    if (msg.value == 0) {
      quoteToken.transferFrom(sender, address(this), amount);
    } else {
      IUniswapV2Router02 router = IUniswapV2Router02(_router);
      address[] memory path = new address[](2);
      path[0] = router.WETH();
      path[1] = _quoteToken;
      router.swapExactETHForTokensSupportingFeeOnTransferTokens{ value: msg.value }(amount, path, address(this), block.timestamp);
    }

    amount = quoteToken.balanceOf(address(this)) - balance;
    require(amount != 0, "Presale: amount may not be 0");

    if (_nonce == 0) {
      participants++;
    }

    if (whitelist) {
      uint256 whitelistDeposit = whitelistDeposits[sender];

      if (whitelistDeposit == 0) {
        whitelistParticipants.push(sender);
      }

      whitelistDeposits[address(0)] = whitelistDeposits[address(0)] + amount; // total
      whitelistDeposits[address(this)] = whitelistDeposits[address(this)] + amount; // total unclaimed
      whitelistDeposits[sender] = whitelistDeposit + amount;
    } else {
      uint256 publicDeposit = publicDeposits[sender];

      if (publicDeposit == 0) {
        publicParticipants.push(sender);
      }

      publicDeposits[address(0)] = publicDeposits[address(0)] + amount; // total
      publicDeposits[address(this)] = publicDeposits[address(this)] + amount; // total unclaimed
      publicDeposits[sender] = publicDeposit + amount;
    }

    emit Deposited(sender, amount, whitelist);
  }

  function finalize() external onlyOwner {
    if (!finalized) {
      finalized = true;
      IERC20 quoteToken = IERC20(_quoteToken);
      uint256 balance = quoteToken.balanceOf(address(this));
      uint256 liquidity = balance * _publicPrice.multiplier / _publicPrice.divider;
      INeedForTurboCoin token = INeedForTurboCoin(_token);
      token.mint(liquidity + whitelistDeposits[address(0)] * _whitelistPrice.multiplier / _whitelistPrice.divider + publicDeposits[address(0)] * _publicPrice.multiplier / _publicPrice.divider);
      token.approve(_router, liquidity);
      quoteToken.approve(_router, balance);
      IUniswapV2Router02 router = IUniswapV2Router02(_router);
      router.addLiquidity(_token, _quoteToken, liquidity, balance, 0, 0, address(this), block.timestamp);
      IERC20 pair = IERC20(IUniswapV2Factory(router.factory()).getPair(_token, _quoteToken));
      balance = pair.balanceOf(address(this));
      uint256 length = _tokenHolders.length;

      for (uint8 i = 0; i < length - 1; i++) {
        pair.transfer(_tokenHolders[i], balance / length);
      }

      pair.transfer(_tokenHolders[length - 1], pair.balanceOf(address(this)));
      emit Finalized();
    }
  }

  function claim(bytes memory signature) external {
    _verifySignature(abi.encode(block.chainid, address(this), "claim"), signature);
    INeedForTurboCoin token = INeedForTurboCoin(_token);
    address sender = _msgSender();
    uint256 whitelistDeposit = whitelistDeposits[sender];
    uint256 publicDeposit = publicDeposits[sender];
    require(whitelistDeposit != 0 || publicDeposit != 0, "Presale: nothing to claim");
    uint256 share = token.balanceOf(address(this)) * (whitelistDeposit * _whitelistPrice.multiplier / _whitelistPrice.divider + publicDeposit * _publicPrice.multiplier / _publicPrice.divider) / (whitelistDeposits[address(this)] * _whitelistPrice.multiplier / _whitelistPrice.divider + publicDeposits[address(this)] * _publicPrice.multiplier / _publicPrice.divider);
    whitelistDeposits[sender] = 0;
    whitelistDeposits[address(this)] = whitelistDeposits[address(this)] - whitelistDeposit;
    publicDeposits[sender] = 0;
    publicDeposits[address(this)] = publicDeposits[address(this)] - publicDeposit;
    token.transfer(sender, share);
    emit Claimed(sender);
  }

  function unlock() external onlyOwner {
    uint256 _timelock;

    unchecked {
      _timelock = block.timestamp + 1 minutes;
    }

    if (_timelock < block.timestamp) {
      _timelock = 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff;
    }

    timelock = _timelock;
    emit Unlocking(_timelock);
  }

  function migrate(address recipient) external onlyOwner {
    require(timelock != 0 && timelock <= block.timestamp, "Presale: not unlocked");
    address _pair = IUniswapV2Factory(IUniswapV2Router02(_router).factory()).getPair(_token, _quoteToken);

    for (uint8 i = 0; i < _tokenHolders.length; i++) {
      TokenHolder(_tokenHolders[i]).withdraw(_pair);
    }

    IERC20 pair = IERC20(_pair);
    pair.transfer(recipient, pair.balanceOf(address(this)));
    emit Migrated(recipient);
  }
}