contract ParmaTransfer {
    using SafeMath for uint256;

    IERC20 public parmaToken = IERC20(0x1A2933fbA0c6e959c9A2D2c933f3f8AD4aa9f06e);

    function noFeeTransfer(uint256 amount, address receiver) external {
        parmaToken.transferFrom(msg.sender, address(this), amount);
        parmaToken.transfer(receiver, amount);
    }
}