    function generateTokenUri(uint256 tokenId, IChameleon.Chameleon memory chameleon)
        external
        pure
        override
        returns (string memory)
    {
        // Generate style tags
        string memory styleTag = _generateStyleTag(chameleon);
        string memory attributes = _generateAttributeTag(chameleon);
        string memory name = string(
            abi.encodePacked(
                'Photonic Fungible Species',
                ' - ',
                Strings.toString(tokenId)
            )
        );

        // Combine with base SVG
        string memory combined = string(
            abi.encodePacked(_svgBeforeStyleTags, styleTag, _svgAfterStyleTags)
        );

        return _formatTokenUri(_svgToImageUri(combined), name, attributes);
    }

    function _valueToHSL(uint16 value) private pure returns (string memory) {
        string memory hue = UintStrings.decimalString(value % 360, 0, false);
        string memory lightness = "65";

        // Default
        if (value == 0) {
            lightness = "100";
        }

        return
            string(
                abi.encodePacked(
                    "hsl(",
                    hue,
                    ",85%,",
                    lightness,
                    "%)"
                )
            );
    }

    function bodyTypeString(uint8 bodyType)
        public
        pure
        returns (string memory)
    {
        return (
            bodyType == 1
                ? "Metachrosis"
                : (bodyType == 2 ? "Camouflage" : "Veiled")
        );
    }

    function environmentTypeString(uint8 backgroundType)
        public
        pure
        returns (string memory)
    {
        return (backgroundType == 1 ? "Nomadic" : "Sedentary");
    }

    function irisTypeString(uint16 eyes) public pure returns (string memory) {
        return (eyes > 0 ? "Recessive" : "Dominant");
    }

    function _generateAttributeTag(IChameleon.Chameleon memory chameleon)
        private
        pure
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    "[{",
                    '"trait_type": "Body",',
                    '"value":"',
                    bodyTypeString(chameleon.bodyType),
                    '"}',
                    ", {",
                    '"trait_type": "Background",',
                    '"value":"',
                    environmentTypeString(chameleon.backgroundType),
                    '"}',
                    ", {",
                    '"trait_type": "Iris",',
                    '"value":"',
                    irisTypeString(chameleon.eyes),
                    '"}',
                    "]"
                )
            );
    }

    function _generateStyleTag(IChameleon.Chameleon memory chameleon)
        private
        pure
        returns (string memory)
    {
        string memory eye = string(
            abi.encodePacked(".eye{fill:", _valueToHSL(chameleon.eyes), ";}")
        );

        string memory body;
        string memory backgroundType;

        if (chameleon.bodyType == 0 || chameleon.bodyType == 1) { // Metachrosis
            string memory bodyColor = _valueToHSL(chameleon.body);
            if (chameleon.eyes > 0) {
                bodyColor = "transparent";
            }
            body = string(
                abi.encodePacked(
                    ".body{fill:",
                    bodyColor,
                    ";}"
                )
            );
        } else if (chameleon.bodyType == 2) { // Camouflage
            body = string(
                abi.encodePacked(
                    ".body{fill: rgba(0,0,0,0);animation: transparent 10s ease-in-out;animation-iteration-count: infinite;}"
                )
            );
        }

        backgroundType = string(
            abi.encodePacked('svg { background: ', _valueToHSL(chameleon.background), ';}')
        );

        string memory styleTag = string(
            abi.encodePacked(
                "<defs><style>", backgroundType,
                "@keyframes transparent { ",
                "0% { fill: ", _valueToHSL(chameleon.body), "; } ",
                "100% { fill: ", _valueToHSL(chameleon.body), "; } ",
                "50% { fill: rgba(0,0,0,0); } }",
                ".tongue{fill:brown;}",
                // ".leaf{fill:#A8C686;}",
                ".leaf{fill:green;}",
                // ".bark{fill:#564D4A;}",
                ".bark{fill:brown;}",
                body,
                eye,
                "</style> </defs>"
            )
        );
        return styleTag;
    }

    function _svgToImageUri(string memory svg)
        private
        pure
        returns (string memory)
    {
        string memory baseURL = "data:image/svg+xml;base64,";
        string memory svgBase64Encoded = Base64.encode(
            bytes(string(abi.encodePacked(svg)))
        );
        return string(abi.encodePacked(baseURL, svgBase64Encoded));
    }

    function _formatTokenUri(
        string memory imageURI,
        string memory name,
        string memory attributes
    ) private pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                name,
                                '", "description":"Hiding in plain sight while strikingly on-chain. Beauty is in the eye of the behodler.", '
                                '"attributes": ',
                                attributes,
                                ',',
                                '"image":"',
                                imageURI,
                                '"}'
                            )
                        )
                    )
                )
            );
    }
